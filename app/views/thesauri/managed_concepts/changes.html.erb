<%= javascript_include_tag 'thesauri/managed_concepts/changes' %>
<%= javascript_tag "var differences_url_path = '#{differences_thesauri_managed_concept_path(@tc)}';" %>
<%= javascript_tag "var changes_url_path = '#{changes_thesauri_managed_concept_path(@tc)}';" %>
<%= javascript_tag "var column_count = '#{@version_count}';" %>
<% second_level_breadcrumb("CDISC Terminology", history_cdisc_terms_path, "Changes") %>
<div class="col-md-12">
  <%= render partial: "shared/thesauri/differences_panel", :locals => {title: "Differences: #{@tc.identifier}, #{@tc.label}", column_count: @version_count, links: @links} %>
  <%= render partial: "shared/thesauri/changes_panel", :locals => {title: "Changes: #{@tc.identifier}, #{@tc.label}", column_count: @version_count, links: @links} %>
  <%= render partial: "shared/buttons/close_v2", :locals => {link_path:  history_cdisc_terms_path, id: "close", small: false} %>
  <%= render partial: "shared/buttons/report", :locals => {link_path:  changes_report_cdisc_term_path(@tc, format: :pdf), id: "report", small: false} %>  
</div>
