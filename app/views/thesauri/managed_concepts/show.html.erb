<%= javascript_include_tag 'thesauri/managed_concepts/show' %>
<%= javascript_tag "var url_path = '#{thesauri_managed_concept_path({id: @tc, managed_concept: {context_id: @context_id}})}';" %>
<div class="row">
  <div class="col-md-8">
    <%= render partial: "shared/iso_managed/managed_header_panel", :locals => {:title => "Show:", :item => @tc} %>
  </div>
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Navigation</h3>
      </div>
      <div class="panel-body">
        <%= back_close_button %>
        <%= link_to "Export CSV", export_csv_thesauri_path(@tc), :class => "btn btn-info" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= render partial: "shared/iso_managed/children_panel", :locals => {:title => "Code Lists"} %>
  </div>
</div>