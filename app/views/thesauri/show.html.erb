<%= javascript_include_tag 'thesauri/show' %>
<%= javascript_tag "var url_path = '#{show_results_thesauri_path(@ct)}';" %>
<% third_level_managed_item_breadcrumb(@ct, "Terminology", thesauri_index_path, @close_path, "Show:") %>
<div class="row">
  <div class="col-md-8">
    <%= render partial: "shared/iso_managed/managed_header_panel", :locals => {:title => "Show:", :item => @ct} %>
  </div>
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Navigation</h3>
      </div>
      <div class="panel-body">
        <%= render partial: "shared/buttons/close_v2", :locals => {link_path: @close_path, id: "close", small: false} %>
        <%= link_to "Export CSV", export_csv_thesauri_path(@ct), :class => "btn btn-info" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= render partial: "shared/iso_managed/managed_children_panel", :locals => {:title => "Code Lists"} %>
  </div>
</div>
