<h3>Index: Biomedical Concepts</h3>
<div class="col-md-6">
		<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
		    	<th>Identifier</th>
		    	<th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @bcs.each do |identifier| %>
				<tr>
			    	<td><%= identifier %></td>
			    	<td><%= link_to "History", history_biomedical_concepts_path(:identifier => identifier), :class => "btn btn-primary btn-xs" %></td>
			  	</tr>
			<% end %>
		</tbody>
	</table>
	<%= link_to "New", new_biomedical_concept_path, :class => "btn btn-success" %>
</div>
