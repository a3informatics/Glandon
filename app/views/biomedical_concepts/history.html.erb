<h3>History: <%= @identifier %></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<%= render partial: "shared/stdInstanceHeader" %>
		<tbody>
			<% @bc.each do |item| %>
				<%= render partial: "shared/stdInstanceRow", :locals => {:item => item, 
	  				:showPath => biomedical_concept_path(:id => item.id, :namespace => item.namespace), 
	  				:searchPath => "", 
	  				:viewPath => "",
	  				:editPath => edit_biomedical_concept_path(:id => item.id, :namespace => item.namespace),
	  				:deletePath => biomedical_concept_path(:id => item.id, :namespace => item.namespace) } %>
			<% end %>				
		</tbody>
	</table>
</div>
