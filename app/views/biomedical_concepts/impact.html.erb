<h3>Biomedical Concept Impact Analysis: <%= @bc.label %> <small>(<%= @bc.owner %>, V<%= @bc.version %>)</small></h3>
<div class="col-md-12">
	<h4>Forms</h4>
	<table id="main"class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
			    <th>Identifer</th>
			    <th>Label</th>
			    <th>Owner</th>
			    <th>Version</th>
			    <th>Version Label</th>
			    <th></th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
		  	<% @forms.each do |id, item| %>
				<tr>
		       		<td><%= item.identifier %></td>
			    	<td><%= item.label %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= link_to "Show", form_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
			    	<td><%= link_to "View", view_forms_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<h4>Domains</h4>
	<table id="sec"class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
			    <th>Identifer</th>
			    <th>Label</th>
			    <th>Owner</th>
			    <th>Version</th>
			    <th>Version Label</th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
		  	<% @domains.each do |id, item| %>
				<tr>
		       		<td><%= item.identifier %></td>
			    	<td><%= item.label %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= link_to "Show", domain_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
