<%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Cloning:", :item => @bc} %>
<div class="col-md-3">
    <%= form_for BiomedicalConcept.new, :url => clone_create_biomedical_concepts_path do |f| %>
    <div class="form-group">
        <%= label_tag :identifier, "Identifier:" %><br />
        <%= f.text_field :identifier, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= label_tag :label, "Label:" %><br />
        <%= f.text_field :label, class: "form-control" %>
    </div>
        <%= f.hidden_field :bc_id, :value => @bc.id %>
        <%= f.hidden_field :bc_namespace, :value => @bc.namespace %>
        <%= f.submit "Clone", class: "btn btn-default " %>
    <% end %>
</div>
