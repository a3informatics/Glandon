<h3>CDISC Biomedical Concepts (All Versions)</h3>
<div class="col-md-10">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>			
			<tr>
				<th>Identifer</th>
				<th>Label</th>
				<th>Owner</th>
				<th>Internal Version</th>
				<th>Version</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @cdiscBcs.each do |bcId, bc| %>
				<tr>
					<td><%= bc.identifier %></td>
					<td><%= bc.label %></td>
					<td><%= bc.owner %></td>
					<td><%= bc.version %></td>
					<td><%= bc.versionLabel %></td>
					<td><%= link_to "Show", cdisc_bc_path(:id => bc.id, :namespace => bc.namespace), :class => "btn btn-primary" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<%= link_to "New Biomedical Concept", new_cdisc_bc_path, :class => "btn btn-success" %>
</div>
