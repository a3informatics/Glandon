<h1>Biomedical Concept <%= @cdiscBc.name %> <small>(CDISC, <%= @cdiscBc.identifier %>, V<%= @cdiscBc.version %>)</small></h1>
<div class="col-md-10">
	<h2>Details</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Identifer</th>
	    <th>Name</th>
	    <th>Version</th>
	  </tr>
	  <tr>
	    <td><%= @cdiscBc.identifier %></td>
	    <td><%= @cdiscBc.name %></td>
	    <td><%= @cdiscBc.version %></td>
	  </tr>
	</table>
	<h2>Items</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
			<% aliasEntry = @cdiscBc.properties.values[0] %>
				<% aliasEntry.each do |key, value| %>
		    	<th><%= key %></th>
				<% end %>
			
		</tr>
		<% @cdiscBc.properties.each do |key, aliasEntry| %>
			<tr>
				<% aliasEntry.each do |key, value| %>
		    	<% if key.to_s == "Values"  %>
						<td>
							<% value.each do |innerValue| %>
							  <% cCode = innerValue[:cCode] %>	
								<i><%= cCode %>: </i>
						    <% clis = innerValue[:clis] %>	
							  <% clis.each do |cliKey, cli| %>
						    	<%= cli.notation %> (<%= cli.id %>)
					  	  <% end %>
				  	    <br/>
				  	  <% end %>
						</td>
		    	<% elsif key.to_s == "Collect"  %>
					  <td>
		    		<% if value %>
							<input type="checkbox" class="checkbox" checked="checked"/>
					  <% else %>
						  <input type="checkbox" class="checkbox"/>
						<% end %>
						</td>
					<% else %>
						<td><%= value %></td>
					<% end %>
				<% end %>
		  </tr>
		<% end %>
	</table>
</div>
