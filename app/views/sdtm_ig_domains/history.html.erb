<%= javascript_tag 'setPageTitle("History: '+@identifier+'")' %>

<%= javascript_pack_tag 'sdtm_ig_domains/history' %>

<% second_level_breadcrumb(:sdtm_ig_domains, @scope_id, @identifier) %>

<div class="row">
  <%= render layout: "shared/headers/header_item_simple", :locals => {badge: "icon-sdtm", item: @item, header_title: "Item History"} do %>
    <div class="actions-wrap">
      <%= render partial: "shared/buttons/ico_btn_secondary", :locals => {link_path: @close_path, id: "close",
        icon: "icon-long-arrow", btn_text: "Return", dt_toggle: "", disabled: false } %>
    </div>
  <% end %>
</div>

<div class="row">
  <%= render partial: "shared/iso_managed/history/history_card", locals: {
        identifier: @identifier, url: history_sdtm_ig_domains_path({sdtm_ig_domain: {identifier: @identifier, scope_id: @scope_id}}) } %>
</div>
  