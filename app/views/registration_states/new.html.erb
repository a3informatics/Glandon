<h1>New Owner</h1>
<div class="col-md-10">
	<%= form_for @registrationState do |f| %>
	  <div class="form-group">
	    <%= f.label :number %><br />
	    <%= f.text_field :number, autofocus: true %>
	  </div>
	  <div class="form-group">
	    <%= f.label :shortName, "Short Name"%><br />
	    <%= f.text_field :shortName %>
	  </div>
	  <div class="form-group">
	    <%= f.label :name, "Long Name" %><br />
	    <%= f.text_field :name %>
	  </div>	  
	  <%= f.submit "Submit", class: "btn btn-default btn-primary" %>  
	<% end %>
</div>