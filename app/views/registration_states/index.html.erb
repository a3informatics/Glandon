<h1>Owner</h1>
<div class="col-md-10">
	<h2>All Versions</h2>
	<table class="table table-striped table-bordered table-condensed">
		  <tr>
		    <th>Number</th>
		    <th>CID</th>
		    <th>Scheme</th>
		    <th>Name</th>
		    <th>Short Name</th>
		    <th></th>
		  </tr>
			<% @registrationStates.each do |registrationState| %>
			  <tr>
			    <td><%= registrationState.number %></td>
			    <td><%= registrationState.id %></td>
			    <td><%= registrationState.scheme %></td>
			    <td><%= registrationState.name %></td>
			    <td><%= registrationState.shortName %></td>
			    <td><%= link_to "Remove", registration_astate_path(registrationState), method: :delete, data: { confirm: "Are you sure?" } %></td>
			  </tr>
			<% end %>
	</table>
	<%= link_to "New state", new_registration_state_path, :class => "btn btn-success" %>
</div>