<%= render partial: "shared/stdChildTitle", :locals => {:title => "Impact Analysis:", :label => @thesaurusConcept.label, :identifier => @thesaurusConcept.identifier} %>
<div class="col-md-12">
	<h4>Details</h4>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Label</th>
	    <th>Identifer</th>
	    <th>Notation</th>
	    <th>Preferred</th>
	    <th>Synonym</th>
	    <th>Definition</th>
	  </tr>
	  <tr>
	    <td><%= @thesaurusConcept.label %></td>
	    <td><%= @thesaurusConcept.identifier %></td>
	    <td><%= @thesaurusConcept.notation %></td>
	    <td><%= @thesaurusConcept.preferredTerm %></td>
	    <td><%= @thesaurusConcept.synonym %></td>
	    <td><%= @thesaurusConcept.definition %></td>
	  </tr>
	</table>	
	<h4>Biomedical Concepts</h4>
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
			    <th>Label</th>
			    <th>Identifer</th>
			    <th>Version</th>
			    <th>Version Label</th>
			    <th>Owner</th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
		  	<% @bcs.each do |id, item| %>
				<tr>
		       		<td><%= item.label %></td>
			    	<td><%= item.identifier %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= link_to "Show", cdisc_bc_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
