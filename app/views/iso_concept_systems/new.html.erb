<h3>New Concept System</h3>
<div class="col-md-3">
	<%= form_for @conceptSystem, :url => iso_concept_systems_path, :method => :post do |f| %>
		<div class="form-group">
	    	<%= f.label :identifier, "Identifier:" %><br />
			<%= f.text_field :identifier, class: "form-control" %>
	  	</div>
	  	<div class="form-group">
	    	<%= label_tag :label, "Label:" %><br />
			<%= f.text_field :label, class: "form-control" %>
	  	</div>
		<div class="form-group">
			<%= f.label :notation, "Notation"%><br />
			<%= f.text_field :notation, class: "form-control" %>
    	</div>
		<%= f.submit "Create", class: "btn btn-default btn-primary" %>
	<% end %>
</div>