<h1>New SDTM Implementation Guide</h1>

<%= form_for @sdtmig, :url => sdtmigs_path do |f| %>
  <div class="form-group">
    <%= label_tag :version, "Version" %><br />
    <%= text_field_tag "form[version]" %>
  </div>
  <div class="form-group">
    <%= label_tag :files, "Files" %><br/>
    <%= f.select :files, @files, {}, { :multiple => true, :size => 10 } %>  
  </div>
  <%= f.submit "Create", class: "btn btn-default btn-primary" %>
<% end %>	
