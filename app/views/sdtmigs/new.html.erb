<h3>New SDTM Implementation Guide</h3>
<div class="col-md-3">
	<%= form_for @sdtmig, :url => sdtmigs_path do |f| %>
  		<div class="form-group">
		    <%= label_tag :versionLabel, "Version Label:" %><br/>
		    <%= text_field_tag "form[versionLabel]", '0.1', class: "form-control" %>
		</div>
		<div class="form-group">
    		<%= label_tag :files, "Files:" %><br/>
    		<%= f.select :files, @files, {}, { :multiple => true, :size => 10 } %>  
  		</div>
  		<%= f.submit "Create", class: "btn btn-default btn-primary" %>
	<% end %>	
</div>
