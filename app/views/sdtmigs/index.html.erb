<h1>CDISC SDTM Implementation Guides</h1>
<div class="col-md-10">
	<h2>All Versions</h2>
	<table id="main" class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Name</th>
	    <th>Internal Version</th>
	    <th>Version</th>
	    <th></th>
	  </tr>
		<% @sdtmigs.each do |key, std| %>
		  <tr>
		    <td><%= std.name %></td>
		    <td><%= std.version %></td>
		    <td><%= std.versionLabel %></td>
		    <td><%= link_to "Show", sdtmig_path(:id => std.id, :namespace => CGI::escape(std.namespace)), :class => "btn btn-primary" %></td>
		  </tr>
		<% end %>
	</table>
	<%= link_to "New version", new_sdtmig_path, :class => "btn btn-success" %>
</div>

