<%= render partial: "shared/stdChildTitle", :locals => {:title => "Impact Analysis:", :label => @cdiscCli.label, :identifier => @cdiscCli.identifier} %>
<div class="col-md-12">
	<h4>Details</h4>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Identifer</th>
	    <th>Notation</th>
	    <th>Preferred</th>
	    <th>Synonym</th>
	    <th>Definition</th>
	  </tr>
	  <tr>
	    <td><%= @cdiscCli.identifier %></td>
	    <td><%= @cdiscCli.notation %></td>
	    <td><%= @cdiscCli.preferredTerm %></td>
	    <td><%= @cdiscCli.synonym %></td>
	    <td><%= @cdiscCli.definition %></td>
	  </tr>
	</table>	
	<h4>Biomedical Concepts</h4>
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
			    <th>Identifer</th>
			    <th>Label</th>
			    <th>Owner</th>
			    <th>Version</th>
			    <th>Version Label</th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
		  	<% @bcs.each do |id, item| %>
				<tr>
		       		<td><%= item.identifier %></td>
			    	<td><%= item.label %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= link_to "Show", biomedical_concept_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<h4>Forms</h4>
	<table id="secondary" class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
			    <th>Identifer</th>
			    <th>Label</th>
			    <th>Owner</th>
			    <th>Version</th>
			    <th>Version Label</th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
		  	<% @forms.each do |id, item| %>
				<tr>
		       		<td><%= item.identifier %></td>
			    	<td><%= item.label %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= link_to "Show", form_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
