<%= javascript_include_tag 'standard_datatable' %>
<% top_level_breadcrumb ("SDTM Domains") %>
<h3>Index: Domains</h3>
<div class="row">
	<div class="col-md-8">
		<table id="main" class="table table-striped table-bordered table-condensed">
			<thead>
				<tr>
		    	<th>Owner</th>
		    	<th>Identifier</th>
		    	<th>Label</th>
		    	<th></th>
		  	</tr>
			</thead>
			<tbody>
				<% @sdtm_user_domains.each do |item| %>
					<tr>
				    	<td><%= item[:owner] %></td>
				    	<td><%= item[:identifier] %></td>
				    	<td><%= item[:label] %></td>
				    	<td><%= link_to "History", history_sdtm_user_domains_path(:sdtm_user_domain => { :identifier => item[:identifier], :scope_id => item[:scope_id] }), 
				    		:class => "btn  btn-xs" %></td>
				  	</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>
