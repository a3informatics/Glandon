<% second_level_breadcrumb("SDTM Domains", sdtm_user_domains_path, "Clone") %>
<%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Cloning:", :item => @sdtm_ig_domain} %>
<div class="col-md-3">
	<%= form_for SdtmUserDomain.new, :url => clone_ig_create_sdtm_user_domains_path do |f| %>
  	<div class="form-group">
      <%= label_tag :prefix, "Prefix:" %><br />
      <%= f.text_field :prefix, :value => "#{@sdtm_ig_domain.prefix}", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :label, "Label:" %><br />
      <%= f.text_field :label, class: "form-control" %>
    </div>
    <%= f.hidden_field :sdtm_ig_domain_id, :value => @sdtm_ig_domain.id %>
    <%= f.hidden_field :sdtm_ig_domain_namespace, :value => @sdtm_ig_domain.namespace %>
    <%= f.submit "Clone", class: "btn btn-default " %>
	<% end %>
</div>