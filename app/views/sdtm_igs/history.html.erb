<%= javascript_include_tag 'standard_datatable' %>
<% top_level_breadcrumb ("CDISC SDTM IGs") %>
<h3>History: CDISC SDTM Implementation Guide</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<%= render partial: "shared/stdInstanceHeader" %>
		<tbody>
			<% current_item = get_current_item(@history) %>
			<%  @history.each do |item| %>
				<%= render partial: "shared/stdInstanceRow", :locals => {:item => item, 
	  				:current_item => current_item, 
	  				:showPath => sdtm_ig_path(:id => item.id, :sdtm_ig => {:namespace => item.namespace} ), 
	  				:searchPath => "", 
	  				:viewPath => "",
	  				:editPath => "",
	  				:deletePath => "",
	  				:mi_class => SdtmIg } %>
			<% end %>				
		</tbody>
	</table>
	<%= link_to "Import", import_sdtm_igs_path, :class => "btn btn-success" %>
</div>
