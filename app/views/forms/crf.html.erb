<% content_for :page_title do %> <%= "CRF: #{@form.has_identifier.identifier} (#{@form.semantic_version})" %> <% end %>

<%= stylesheet_link_tag 'shared/forms/form_crf' %>

<% third_level_breadcrumb(:forms, @form, @close_path) %>

<div class="row">
  <%= render layout: "shared/headers/header_managed_item", :locals => { item: @form, badge: "icon-forms" } do %>
    <div class="actions-wrap">
      <%= render partial: "shared/buttons/return", :locals => { url: @close_path } %>
    </div>
  <% end %>
</div>

<div class="row">

  <div id="crf-panel" class="card col-md-12">

    <div class="card-header">
      CRF View
    </div>

    <div class="card-content">
      <%= raw @html %>
    </div>

  </div>
</div>
