<h3>Forms</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
		  	<tr> 
		    	<th>Identifer</th>
				<th>Label</th>
				<th>Owner</th>
				<th>Registration Status</th>
				<th>Version</th>
				<th>Internal Version</th>
				<th></th>
				<th></th>
				<th></th>
				<th></th>
		  	</tr>
	  	</thead>
	  	<tbody>
			<% @forms.each do |formId, form| %>
		  		<tr>
		    		<td><%= form.identifier %></td>
					<td><%= form.label %></td>
					<td><%= form.owner %></td>
					<td><%= form.registrationStatus %></td>
					<td><%= form.versionLabel %></td>
					<td><%= form.version %></td>
					<td><%= link_to "Show", form_path(:id => form.id, :namespace => form.namespace), :class => "btn btn-primary btn-xs" %></td>
					<td><%= link_to "View", view_forms_path(:id => form.id, :namespace => form.namespace), :class => "btn btn-primary btn-xs" %></td>
					<td><%= link_to "Status", edit_iso_registration_state_path(:id => form.registrationState.id), :class => "btn btn-primary btn-xs" %></td>
					<td><%= link_to "Triples", view_dashboard_index_path(:id => form.id, :namespace => form.namespace), :class => "btn btn-warning btn-xs" %></td>
		  		</tr>
			<% end %>
		</tbody>
	</table>
	<!-- <%= link_to "New Form", new_form_path, :class => "btn btn-success" %>-->
	<%= link_to "New Form", new_form_path, :class => "btn btn-success" %>
	<%= link_to "New Placeholder Form", placeholder_new_forms_path, :class => "btn btn-success" %>
	<%= link_to "New Normal BC Form", bc_normal_new_forms_path, :class => "btn btn-success" %>
	<%= link_to "New Log Line BC Form", bc_log_new_forms_path, :class => "btn btn-success" %>
</div>
