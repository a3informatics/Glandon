<%= stylesheet_link_tag    'd3_graph' %>
<%= javascript_include_tag 'd3_graph', 'iso_managed_graph' %>
<script id="jsonData" type="text/json">
    <%= raw @result.to_json %>
</script>
<div class="row">
	<div class="col-md-11">
    <%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Metadata Graph:", :item => @item} %>
    <div style="height: 1000px;" id="d3">
        <!-- D3 Area -->
    </div>
   </div>
	<div class="col-md-1">
		<br/>
		<button id="graph_stop" type="button" class="btn btn-default btn-primary">
  		<span class="glyphicon glyphicon-pause" aria-hidden="true"></span>
		</button>
	</div>
</div>