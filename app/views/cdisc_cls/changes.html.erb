<h3>Changes: <%= @title %> <small><%= @identifier %></small></h3>
<div class="col-md-12">
	<h4>Code List Changes</h4>	
	<style>
	  <%= Diffy::CSS %>
	</style>
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  <tr>
		    <th>Version</th>
		    <th class="col-sm-1">Date</th>
		    <th class="col-sm-1">Identifier</th>
		    <th class="col-sm-1">Notation</th>
		    <th class="col-sm-2">Preferred Term</th>
		    <th class="col-sm-2">Synonym</th>
  			<th class="col-sm-1">Extensible</th>
		    <th class="col-sm-3">Definition</th>
  	  </tr>	
		<thead>
		<tbody>
			<% @ClResults.each do |r| %>
				<tr>
			    <td><%= raw r["version"] %></td>
			    <td><%= raw r["date"] %></td>
			    <td><%= raw r["identifier"] %></td>
		      <td><%= raw r["notation"] %></td>
		      <td><%= raw r["preferredTerm"] %></td>
		      <td><%= raw r["synonym"] %></td>
		      <td><%= raw r["extensible"] %></td>
		      <td><%= raw r["definition"] %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
	<h4>Code List Item Changes</h4>	
	<table id="secondary" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>Code List Item</th>
				<th>Submission Value</th>
				<% cliEntry = @CliResults.values[0] %>
				<% value = cliEntry[:result]%>
				<% value.each do |vKey, value| %>
			    <th><%= vKey %></th>
			  <% end %>
				<th></th>
			</tr>
		</thead>
		<tbody>	
			<% @CliResults.each do |cliKey, cliEntry| %>
				<% value = cliEntry[:result]%>
				<% cli = cliEntry[:cli]%>
				<tr>
					<td><%= cli.identifier %></td>
					<td><%= cli.notation %></td>
					<% value.each do |vKey, status| %>
						<%= render partial: "shared/historyCell", :locals => {:historyValue => status} %>
					<% end %>
					<td><%=link_to "Changes", changes_cdisc_clis_path(:id => cli.id), :class => "btn btn-success btn-xs" %></td>
				</tr>	
			<% end %>
		</tbody>
	</table>
</div>
