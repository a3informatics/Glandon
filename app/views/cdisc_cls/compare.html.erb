<h3><%= @title %> <small>(CDISC, <%= @identifier %>)</small></h3>
<div class="col-md-12">
	<h4>Code List Version Comparison</h4>
	<style>
	  	<%= Diffy::CSS %>
	</style>
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
		  	<tr>
			    <th>Version</th>
			    <th class="col-sm-1">Date</th>
			    <th class="col-sm-1">Identifier</th>
			    <th class="col-sm-1">Notation</th>
			    <th class="col-sm-2">Preferred Term</th>
			    <th class="col-sm-2">Synonym</th>
			    <th class="col-sm-2">Extensible</th>
			    <th class="col-sm-4">Definition</th>
		  	</tr>
		</thead>
		<tbody>
			<% @ClResults.each do |r| %>
				<tr>
		    		<td><%= raw r["version"] %></td>
				    <td><%= raw r["date"] %></td>
				    <td><%= raw r["identifier"] %></td>
				    <td><%= raw r["notation"] %></td>
				    <td><%= raw r["preferredTerm"] %></td>
				    <td><%= raw r["synonym"] %></td>
				    <td><%= raw r["extensible"] %></td>
				    <td><%= raw r["definition"] %></td>
		  		</tr>
			<% end %>
		</tbody>
	</table>
	<h4>Code List Items</h4>
	<table id="secondary" class="table table-striped table-bordered table-condensed">
	  	<thead>
	  		<tr>
				<th>Identifier</th>
				<th>Label</th>
				<th>Notation</th>
				<% cliEntry = @CliResults.values[0] %>
				<% result = cliEntry[:result]%>
				<% result.each do |vKey, value| %>
			    	<th class="text-center"><%= vKey %></th>
			  	<% end %>
				<th></th>
			</tr>
		</thead>
		<tbody>	
			<% @CliResults.each do |cliId, cliEntry| %>
				<% result = cliEntry[:result]%>
				<% cli = cliEntry[:cli]%>
				<tr>
					<td><%= cli.identifier %></td>
					<td><%= cli.label %></td>
					<td><%= cli.notation %></td>
					<% result.each do |vKey, status| %>
			    		<% if status == "." %>
							<td class="text-center"><span class="glyphicon glyphicon-ok-circle"/></td>
						<% elsif status == "M"  %>
							<td class="text-center bg-warning">
								<span class="text-center glyphicon glyphicon-pencil"/>
							</td>
						<% elsif status == "X"  %>
							<td class="text-center bg-danger">	
								<span class="glyphicon glyphicon-remove-circle"/>
							</td>
						<% else %>
							<td class="text-center">&nbsp;</td>
						<% end %>
					<% end %>
					<td><%=link_to "Compare", compare_cdisc_clis_path(:id => @newCl.id, :oldTermId => @oldCdiscTerm.id, :oldTermNs => @oldCdiscTerm.namespace, :newTermId => @newCdiscTerm.id, :newTermNs => @newCdiscTerm.namespace, :type => "ALL"), :class => "btn btn-primary btn-xs" %></td>
				</tr>	
			<% end %>
		</tbody>
	</table>
</div>
