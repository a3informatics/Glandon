<h1><%= @cdiscCl.preferredTerm %> <small>(CDISC, <%= @cdiscCl.identifier %>, <%= @cdiscTerm.date %>, V<%= @cdiscTerm.version %>)</small></h1>
<div class="col-md-7">
	<h2>Details</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <thead>
			<tr>
		    <th>Identifer</th>
		    <th>Notation</th>
		    <th>Preferred</th>
		    <th>Synonym</th>
		    <th>Extensible</th>
		    <th>Definition</th>
		  </tr>
		</thead>
		<tbody>
		  <tr>
		    <td><%= @cdiscCl.identifier %></td>
		    <td><%= @cdiscCl.notation %></td>
		    <td><%= @cdiscCl.preferredTerm %></td>
		    <td><%= @cdiscCl.synonym %></td>
		    <td><%= @cdiscCl.extensible %></td>
		    <td><%= @cdiscCl.definition %></td>
		  </tr>
		</tbody>
	</table>
	<h2>Code List Items</h2>
	<table id="cli" class="table table-striped table-bordered table-condensed">
	  <thead>
			<tr>
		    <th>Identifer</th>
		    <th>Notation</th>
		    <th>Preferred</th>
		    <th>Synonym</th>
		    <th>Definition</th>
		    <th>View</th>
	  	</tr>
		</thead>
		<tbody>
			<% @cdiscClis.each do |id, cli| %>
			  <tr>
			    <td><%= cli.identifier %></td>
			    <td><%= cli.notation %></td>
			    <td><%= cli.preferredTerm %></td>
			    <td><%= cli.synonym %></td>
			    <td><%= cli.definition %></td>
			    <td><%= link_to "Show", cdisc_cli_path(:id => cli.id,:termId => @cdiscTerm.id), :class => "btn btn-primary" %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
</div>
<div class="col-md-3">
	<%= render partial: "shared/previousHistory", :locals => {:type => "CL", :object => @cdiscCl, :currTerm => @cdiscTerm, :cdiscTerms => @cdiscTerms} %>
	<%= render partial: "shared/fullHistory", :locals => {:type => "CL", :object => @cdiscCl, :currTerm => @cdiscTerm} %>
</div>
