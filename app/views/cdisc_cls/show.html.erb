<%= render partial: "shared/stdChildTitle", :locals => {:title => "Show:", :label => @cdiscCl.label, :identifier => @cdiscCl.identifier} %>
<div class="col-md-12">
	<h4>Details</h4>
	<table class="table table-striped table-bordered table-condensed">
	  	<thead>
			<tr>
			    <th>Label</th>
			    <th>Identifer</th>
			    <th>Notation</th>
			    <th>Preferred</th>
			    <th>Synonym</th>
			    <th>Extensible</th>
			    <th>Definition</th>
		  	</tr>
		</thead>
		<tbody>
		  <tr>
		    <td><%= @cdiscCl.label %></td>
		    <td><%= @cdiscCl.identifier %></td>
		    <td><%= @cdiscCl.notation %></td>
		    <td><%= @cdiscCl.preferredTerm %></td>
		    <td><%= @cdiscCl.synonym %></td>
		    <td><%= @cdiscCl.extensible %></td>
		    <td><%= @cdiscCl.definition %></td>
		  </tr>
		</tbody>
	</table>
	<h4>Code List Items</h4>
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
			<tr>
			    <th>Identifer</th>
			    <th>Notation</th>
			    <th>Preferred</th>
			    <th>Synonym</th>
			    <th>Definition</th>
			    <th></th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @cdiscCl.children.each do |id, cli| %>
			  <tr>
			    <td><%= cli.identifier %></td>
			    <td><%= cli.notation %></td>
			    <td><%= cli.preferredTerm %></td>
			    <td><%= cli.synonym %></td>
			    <td><%= cli.definition %></td>
			    <td><%= link_to "Show", cdisc_cli_path(:id => cli.id, :namespace => cli.namespace), :class => "btn btn-primary btn-xs" %></td>
			    <td><%= link_to "Impact", impact_cdisc_clis_path(:id => cli.id,  :namespace => cli.namespace), :class => "btn btn-info btn-xs" %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
	<%= link_to "Changes", changes_cdisc_cls_path(:id => @cdiscCl.id, :namespace => @cdiscCl.namespace), :class => "btn btn-success" %>
</div>
