<h3><%= @cdiscCl.preferredTerm %> <small>(CDISC, <%= @cdiscCl.identifier %>, <%= @cdiscTerm.versionLabel %>, V<%= @cdiscTerm.version %>)</small></h3>
<div class="col-md-8">
	<h4>Details</h4>
	<table class="table table-striped table-bordered table-condensed">
	  <thead>
			<tr>
		    <th>Identifer</th>
		    <th>Notation</th>
		    <th>Preferred</th>
		    <th>Synonym</th>
		    <th>Extensible</th>
		    <th>Definition</th>
		  </tr>
		</thead>
		<tbody>
		  <tr>
		    <td><%= @cdiscCl.identifier %></td>
		    <td><%= @cdiscCl.notation %></td>
		    <td><%= @cdiscCl.preferredTerm %></td>
		    <td><%= @cdiscCl.synonym %></td>
		    <td><%= @cdiscCl.extensible %></td>
		    <td><%= @cdiscCl.definition %></td>
		  </tr>
		</tbody>
	</table>
	<h4>Code List Items</h4>
	<table id="cli" class="table table-striped table-bordered table-condensed">
	  	<thead>
			<tr>
			    <th>Identifer</th>
			    <th>Notation</th>
			    <th>Preferred</th>
			    <th>Synonym</th>
			    <th>Definition</th>
			    <th></th>
			    <th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @cdiscClis.each do |id, cli| %>
			  <tr>
			    <td><%= cli.identifier %></td>
			    <td><%= cli.notation %></td>
			    <td><%= cli.preferredTerm %></td>
			    <td><%= cli.synonym %></td>
			    <td><%= cli.definition %></td>
			    <td><%= link_to "Show", cdisc_cli_path(:id => cli.id, :termId => @cdiscTerm.id), :class => "btn btn-primary btn-xs" %></td>
			    <td><%= link_to "Impact", impact_cdisc_clis_path(:id => cli.id,  :termId => @cdiscTerm.id), :class => "btn btn-info btn-xs" %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
</div>
<div class="col-md-4">
	<%= render partial: "shared/previousHistory", :locals => {:type => "CL", :object => @cdiscCl, :currTerm => @cdiscTerm, :cdiscTerms => @cdiscTerms} %>
	<%= render partial: "shared/fullHistory", :locals => {:type => "CL", :object => @cdiscCl, :currTerm => @cdiscTerm} %>
</div>
