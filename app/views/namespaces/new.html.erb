<h1>New Namespace</h1>

<%= form_for @namespace do |f| %>
   <div class="form-group">
    <%= f.label :shortName, "Short Name"%><br />
    <%= f.text_field :shortName, autofocus: true %>
  </div>
  <div class="form-group">
    <%= f.label :name, "Name"%><br />
    <%= f.text_field :name%>
  </div>
  <%= f.submit "Submit", class: "btn btn-default btn-primary" %>  
<% end %>

