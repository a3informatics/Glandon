<h1>New Namespace</h1>

<%= form_for @namespace do |f| %>
   <div class="field">
    <%= f.label :shortName "Short Name"%><br />
    <%= f.text_field :shortName, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :name "Name"%><br />
    <%= f.text_field :name%>
  </div>
  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>

