<h3>Sponsor Terminology (All Versions)</h3>
<div class="col-md-10">
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Internal Version</th>
	    <th>Version Label</th>
	    <th></th>
	  </tr>
		<% @sponsorTerms.each do |sponsorTerm| %>
		  <tr>
		    <td><%= sponsorTerm.version %></td>
		    <td><%= sponsorTerm.versionLabel %></td>
		    <td><%= link_to "Show", sponsorTerm, :class => "btn btn-primary" %></td>
		  </tr>
		<% end %>
	</table>
	<%= link_to "New version", new_sponsor_term_path, :class => "btn btn-success" %>
</div>

