<h3>Sponsor Terminology</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  	<tr>
		    	<th>Internal Version</th>
		    	<th>Version Label</th>
		    	<th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @sponsorTerms.each do |sponsorTerm| %>
			  	<tr>
			    	<td><%= sponsorTerm.version %></td>
			    	<td><%= sponsorTerm.versionLabel %></td>
			    	<td><%= link_to "Show", sponsorTerm, :class => "btn btn-primary btn-xs" %></td>
			  	</tr>
			<% end %>
		</tbody>
	</table>
	<%= link_to "New version", new_sponsor_term_path, :class => "btn btn-success" %>
</div>

