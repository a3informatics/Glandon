<h1>sponsor Terminology <%= @sponsorTerm.versionLabel %> <small>(V<%= @sponsorTerm.version %>)</small></h1>
<div class="col-md-10">
	<h2>Details</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <thead>
	  <tr>
	    <th>Identifer</th>
	    <th>Version</th>
	    <th>Version Label</th>
	  </tr>
		</thead>
		<tbody>
	  <tr>
	    <td><%= @sponsorTerm.identifier %></td>
	    <td><%= @sponsorTerm.version %></td>
	    <td><%= @sponsorTerm.versionLabel %></td>
	  </tr>
	</tbody>
	</table>
	<h2>Code Lists</h2>
	<table id="cl" class="table table-striped table-bordered table-condensed">
	  <thead>
		  <tr>
		    <th>Identifer</th>
		    <th>Submission Value</th>
		    <th>Preferred Term</th>
		    <th>Synonym(s)</th>
		    <th>Extensible</th>
		    <th>Definition</th>
		    <th>View</th>
		  </tr>
		</thead>
		<tfoot>
      <tr>
		    <th></th>
		    <th></th>
		    <th></th>
		    <th></th>
		    <th></th>
		    <th></th>
		    <th></th>
      </tr>
    </tfoot>
		<tbody>
			<% @sponsorTerm.thesaurus.children.each do |key, cl| %>
		  	<tr>
		    	<td><%= cl.identifier %></td>
			    <td><%= cl.notation %></td>
			    <td><%= cl.preferredTerm %></td>
			    <td><%= cl.synonym %></td>
			    <td><%= cl.extensible %></td>
			    <td><%= cl.definition %></td>
			    <td><%= link_to "Show", thesaurus_concept_path(:id => cl.id, :termId => @sponsorTerm.id), :class => "btn btn-primary" %></td>
		  	</tr>
			<% end %>
		</tbody>
	</table>
</div>

