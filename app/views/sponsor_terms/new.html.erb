<h3>New Sponsor Terminology Version</h3>
<div class="col-md-10">
  <%= form_for @sponsorTerm do |f| %>
      <div class="form-group">
          <%= label_tag :version, "Version (internal system)" %><br/>
          <%= text_field_tag "cdisc_term[version]" %>
      </div>
      <div class="form-group">
        <%= label_tag :date, "Date associated with the terminology release" %><br/>
        <%= date_field_tag "cdisc_term[date]" %>
      </div>
      <%= f.label :files %><br/>
      <%= f.select :files, @files, {}, { :multiple => true, :size => 10 } %><br/>  
      <br/>
      <%= f.submit "Create", class: "btn btn-default btn-primary" %>
  <% end %>
</div>
