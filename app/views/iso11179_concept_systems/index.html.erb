<h3>Classification (Concept) Systems</h3>
<div class="col-md-12">
	<h4>All Systems</h4>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Name</th>
	    <th></th>
	    <th></th>
	  </tr>
		<% @conceptSystems.each do |csId, cs| %>
		  <tr>
		    <td><%= cs.notation %></td>
		    <td><%= link_to "Show", iso11179_concept_system_path(cs.id), :class => "btn btn-primary btn-xs" %></td>
		    <td><%= link_to "Delete", iso11179_concept_system_path(cs.id), method: :delete, data: { confirm: "Are you sure?" }, :class => "btn btn-danger btn-xs" %></td>
		  </tr>
		<% end %>
	</table>
	<%= link_to "New System", new_iso11179_concept_system_path, :class => "btn btn-success" %>
</div>
