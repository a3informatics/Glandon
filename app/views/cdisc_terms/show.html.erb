<h1>CDISC Terminology (V<%= @cdiscTerm.version %>, <%= @cdiscTerm.date %>)</h1>
<table>
	<td>
		<h2>Details</h2>
			<table>
			  <tr>
			    <th>Identifer</th>
			    <th>Version</th>
			    <th>Date</th>
			  </tr>
			  <tr>
			    <td><%= @cdiscTerm.identifier %></td>
			    <td><%= @cdiscTerm.version %></td>
			    <td><%= @cdiscTerm.date %></td>
			  </tr>
			</table>
			<h2>Code Lists</h2>
			<table>
			  <tr>
			    <th>Identifer</th>
			    <th>Notation</th>
			    <th>Preferred</th>
			    <th>Synonym</th>
			    <th>Extensible</th>
			    <th>Definition</th>
			    <th>View</th>
			  </tr>
				<% @CdiscCls.each do |cl| %>
				  <tr>
				    <td><%= cl.identifier %></td>
				    <td><%= cl.notation %></td>
				    <td><%= cl.preferredTerm %></td>
				    <td><%= cl.synonym %></td>
				    <td><%= cl.extensible %></td>
				    <td><%= cl.definition %></td>
				    <td><%= link_to "Show", cdisc_cl_path(:id => cl.id, :termId => @cdiscTerm.id) %></td>
				  </tr>
				<% end %>
			</table>
	</td>
	<td>	
		<h2>Compare to Specific Version</h2>
		<table>
		  <tr>
		    <th>Version</th>
		    <th>Date</th>
		    <th></th>
		  </tr>
			<% @cdiscTerms.each do |ct| %>
			  <tr>
			    <td><%= ct.version %></td>
			    <td><%= ct.date %></td>
			    <td><%= link_to "Compare", compare_cdisc_terms_path(:new => @cdiscTerm.id, :old => ct.id) %></td>
			  </tr>
			<% end %>
		</table>
		<br>
		<h2>Get History</h2>
		<%= link_to "History", history_cdisc_terms_path(:id => @cdiscTerm.id) %>
	</td>
</table>