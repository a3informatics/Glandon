<h3>Impact: CDISC Terminology <%= @new_cdisc_term.versionLabel %> versus <%= @old_cdisc_term.versionLabel %> <small>(V<%= @new_cdisc_term.version %> versus V<%= @old_cdisc_term.version %>)</small></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
	  		<tr>
				<th>Code List</th>
				<th>Item</th>
				<th>Old Submission</th>
				<th>New Submission</th>
				<th>Biomedical Concept</th>
				<th></th>
				<th>Operational</th>
				<th></th>
			</tr>
		</thead>
		<tbody>	
			<% @results.each do |cli| %>
				<% identifier = cli["identifier"] %>
				<% parent_identifier = cli["parent_identifier"] %>
				<% old_sub = cli["old_notation"] %>
				<% new_sub = cli["new_notation"] %>
				<% bcs = cli["bcs"] %>
				<% bcs.each do |bc| %>
					<% bc_identifier = bc["identifier"] %>
					<% bc_label = bc["label"] %>
					<% forms = bc["forms"] %>
					<% if forms.length > 0 %>
						<% forms.each do |form| %>
							<% form_identifier = form["identifier"] %>
							<% form_label = form["label"] %>
							<tr>
								<td><%= parent_identifier %></td>
								<td><%= identifier %></td>
								<td><%= old_sub %></td>
								<td><%= new_sub %></td>
								<td><%= bc_label + " (" + bc_identifier + ")" %></td>
								<td><%=link_to "Show", biomedical_concept_path(:id => bc["id"], :namespace => bc["ns"]), :class => "btn btn-primary btn-xs" %></td>
								<td><%= form_label + " (" + form_identifier + ")" %></td>
								<td><%=link_to "View", view_forms_path(:id => form["id"], :namespace => form["ns"]), :class => "btn btn-primary btn-xs" %></td>
							</tr>	
						<% end %>
					<% end %>
					<% domains = bc["domains"] %>
					<% if domains.length > 0 %>
						<% domains.each do |domain| %>
							<% domain_identifier = domain["identifier"] %>
							<% domain_label = domain["label"] %>
							<tr>
								<td><%= parent_identifier %></td>
								<td><%= identifier %></td>
								<td><%= old_sub %></td>
								<td><%= new_sub %></td>
								<td><%= bc_label + " (" + bc_identifier + ")" %></td>
								<td><%=link_to "Show", biomedical_concept_path(:id => bc["id"], :namespace => bc["ns"]), :class => "btn btn-primary btn-xs" %></td>
								<td><%= domain_label + " (" + domain_identifier + ")" %></td>
								<td><%=link_to "Show", domain_path(:id => domain["id"], :namespace => domain["ns"]), :class => "btn btn-primary btn-xs" %></td>
							</tr>	
						<% end %>
					<% end %>
					<% if forms.length == 0 && domains.length == 0 %>
						<tr>
							<td><%= parent_identifier %></td>
							<td><%= identifier %></td>
							<td><%= old_sub %></td>
							<td><%= new_sub %></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
						</tr>	
					<% end %>
				<%end %>
			<% end %>
		</tbody>
	</table>
	<%= link_to "Report", impact_report_cdisc_terms_path(:format => :pdf, :new_version => @new_version, :old_version => @old_version) , :class => "btn btn-primary", :target => "_blank" %>
</div>
