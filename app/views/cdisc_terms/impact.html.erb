<h3>Impact: CDISC Terminology <%= @new_cdisc_term.versionLabel %> versus <%= @old_cdisc_term.versionLabel %> <small>(V<%= @new_cdisc_term.version %> versus V<%= @old_cdisc_term.version %>)</small></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
	  		<tr>
				<th>Code List</th>
				<th>Item</th>
				<th>Label</th>
				<th>Old Submission</th>
				<th>New Submission</th>
				<th>Impact Item Type</th>
				<th>Impact Item Label</th>
				<th>Impact Item Identifier</th>
        <th>Via</th>
        <th></th>
			</tr>
		</thead>
		<tbody> 
      <% @results.each do |result| %>
        <tr>
          <td><%= result[:parent_identifier] %></td>
          <td><%= result[:identifier] %></td>
          <td><%= result[:label] %></td>
          <td><%= result[:old_notation] %></td>
          <td><%= result[:new_notation] %></td>
          <td><%= result[:item_type] %></td>
          <td><%= result[:item_label] %></td>
          <td><%= result[:item_identifier] %></td>
          <td><%= result[:item_via] %></td>
          <% if result[:item_type] == "Biomedical Concept" %>
          	<td><%=link_to "Show", biomedical_concept_path(:id => result[:item_id], :namespace => result[:namespace]), :class => "btn btn-primary btn-xs" %></td>
					<% elsif result[:item_type] == "Form" %>
          	<td><%=link_to "View", view_forms_path(:id => result[:item_id], :namespace => result[:namespace]), :class => "btn btn-primary btn-xs" %></td>
          <% elsif result[:item_type] == "Domain" %>
						<td><%=link_to "Show", domain_path(:id => result[:item_id], :namespace => result[:namespace]), :class => "btn btn-primary btn-xs" %></td>
          <% else %>
          	<td></td>
          <% end %>
        </tr> 
      <% end %>
    </tbody>
	</table>
	<%= link_to "Report", impact_report_cdisc_terms_path(:format => :pdf, :new_version => @new_version, :old_version => @old_version) , :class => "btn btn-primary", :target => "_blank" %>
</div>
