<% second_level_breadcrumb("CDISC Terminology", history_cdisc_terms_path, "Import") %>
<h3>New CDISC Terminology Version</h3>
<div class="col-md-6">
  <%= form_for @cdiscTerm do |f| %>
      <div class="form-group">
          <%= label_tag :version, "Version:" %><br/>
          <%= text_field_tag "cdisc_term[version]", "#{@next_version}", class: "form-control" %>
      </div>
      <div class="form-group">
        <%= label_tag :date, "Terminology release date:" %><br/>
        <%= date_field_tag "cdisc_term[date]", class: "form-control" %>
      </div>
      <div class="form-group">
        <%= label_tag :files, "Files:" %><br/>
        <%= select_tag("cdisc_term[files][]", options_for_select(@files), class: "form-control", :multiple => true) %>
      </div>
      <br/>
      <%= f.submit "Create", class: "btn btn-default btn-primary" %>
  <% end %>
</div>
