<h3>History: CDISC Terminology</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
	  		<tr>
		    	<th>Label</th>
		    	<th>Identifier</th>
		    	<th>Version</th>
		    	<th>Version Label</th>
		    	<th>Owner</th>
		    	<th></th>
		    	<th></th>
		    	<th></th>
	  		</tr>
	  	</thead>
	  	<tbody>
	  		<% @cdiscTerms.each do |key, item| %>
			  	<tr>
			    	<td><%= item.label %></td>
			    	<td><%= item.identifier %></td>
			    	<td><%= item.version %></td>
			    	<td><%= item.versionLabel %></td>
			    	<td><%= item.owner %></td>
			    	<td><%= link_to "Show", cdisc_term_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
			    	<td><%= link_to "Search", search_cdisc_terms_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-primary btn-xs" %></td>
			    	<td><%= link_to "Triples", view_dashboard_index_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-warning btn-xs" %></td>
			  	</tr>
			<% end %>	
	  	</tbody>
	</table>
	<%= link_to "Import", import_cdisc_terms_path, :class => "btn btn-success" %>
	<%= link_to "Changes", changes_cdisc_terms_path() , :class => "btn btn-success" %>
</div>
