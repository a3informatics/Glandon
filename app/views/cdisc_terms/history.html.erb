<%= javascript_include_tag 'standard_datatable' %>
<% top_level_breadcrumb ("CDISC Terminology") %>
<h3>History: CDISC Terminology</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<%= render partial: "shared/stdInstanceHeader" %>
		<tbody>
			<% current_item = get_current_item( @cdiscTerms) %>
			<% @cdiscTerms.each do |item| %>
				<%= render partial: "shared/std_instance_row_v2", :locals => {:item => item, 
	  				:current_item => current_item, 
	  				:showPath => cdisc_term_path(item), 
	  				:searchPath => search_cdisc_terms_path(item), 
	  				:viewPath => "",
	  				:editPath => "",
	  				:deletePath => "",
	  				:index_label => "",
	  				:index_path => "",
	  				:mi_class => CdiscTerm } %>
			<% end %>				
		</tbody>
	</table>
	<%= link_to "Changes", changes_cdisc_term_path(@cdiscTerms.first), :class => "btn btn-primary" %>
	<%= link_to "Submission", submission_calc_cdisc_terms_path() , :class => "btn btn-primary" %>
</div>
