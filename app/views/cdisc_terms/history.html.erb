<h3>History: CDISC Terminology</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<%= render partial: "shared/stdInstanceHeader" %>
		<tbody>
			<% current_item = get_current_item( @cdiscTerms) %>
			<%  @cdiscTerms.each do |item| %>
				<%= render partial: "shared/stdInstanceRow", :locals => {:item => item, 
	  				:current_item => current_item, 
	  				:showPath => cdisc_term_path(:id => item.id, :namespace => item.namespace), 
	  				:searchPath => search2_cdisc_terms_path(:id => item.id, :namespace => item.namespace), 
	  				:viewPath => "",
	  				:editPath => "",
	  				:clonePath => "",
	  				:deletePath => "" } %>
			<% end %>				
		</tbody>
	</table>
	<%= link_to "Changes", changesCalc_cdisc_terms_path() , :class => "btn btn-primary" %>
	<%= link_to "Submission", submissionCalc_cdisc_terms_path() , :class => "btn btn-primary" %>
	<%= link_to "Import", import_cdisc_terms_path, :class => "btn btn-success" %>
	<%= link_to "Load", load_cdisc_terms_path, :class => "btn btn-success" %>
	<%= link_to "Files", file_cdisc_terms_path, :class => "btn btn-primary" %>
</div>
