<h1><%= @title %> <small>(CDISC, <%= @identifier %>)</small></h1>
<div class="col-md-10">
	<h2>Terminology Release History</h2>	
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
			<th>Code List</th>
			<th>Submission Value</th>
			<% clEntry = @Results.values[0] %>
			<% result = clEntry[:result]%>
			<% result.each do |vKey, status| %>
		    <th><%= vKey %></th>
		  <% end %>
			<th>View History</th>
		</tr>	
		<% @Results.each do |clKey, clEntry| %>
			<% result = clEntry[:result]%>
			<% identifier = clEntry[:identifier]%>
			<% clId = clEntry[:id]%>
			<tr>
				<td><%= clKey %></td>
				<td><%= identifier %></td>
				<% result.each do |vKey, status| %>
					<%= render partial: "shared/historyCell", :locals => {:historyValue => status} %>
				<% end %>
				<td><%=link_to "History", history_cdisc_cls_path(:id => clId), :class => "btn btn-info" %></td>
			</tr>	
		<% end %>
	</table>
</div>
