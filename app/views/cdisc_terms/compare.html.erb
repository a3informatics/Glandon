<h3><%= @title %> <small>(CDISC, <%= @identifier %>)</small></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  <tr>
				<th>Code List Item</th>
				<% clEntry = @Results.values[0] %>
				<% value = clEntry[:result]%>
				<% value.each do |vKey, value| %>
			    	<th><%= vKey %></th>
			  	<% end %>
				<th>View History</th>
			</tr>
		</thead>
		<tbody>
			<% @Results.each do |clKey, clEntry| %>
				<% value = clEntry[:result]%>
				<% clId = clEntry[:id]%>
				<tr>
					<td><%= clKey %></td>
					<% value.each do |vKey, status| %>
			    	<%= render partial: "shared/historyCell", :locals => {:historyValue => status} %>
					<% end %>
					<td><%=link_to "History", history_cdisc_cls_path(:id => clId), :class => "btn btn-info btn-xs" %></td>
				</tr>	
			<% end %>
		</tbody>
	</table>
</div>
