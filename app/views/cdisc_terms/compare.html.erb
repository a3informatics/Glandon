<h3>Compare: <%= @newCdiscTerm.label %> <small>(<%= @newCdiscTerm.identifier %>)</small></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
		  <tr>
				<th>Identifier</th>
				<th>Label</th>
				<th>Notation</th>
				<% clEntry = @Results.values[0] %>
				<% value = clEntry[:result]%>
				<% value.each do |vKey, value| %>
			    	<th><%= vKey %></th>
			  	<% end %>
				<th></th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @Results.each do |clKey, clEntry| %>
				<% value = clEntry[:result]%>
				<% cl = clEntry[:cl]%>
				<tr>
					<td><%= cl.identifier %></td>
					<td><%= cl.label %></td>
					<td><%= cl.notation %></td>
					<% value.each do |vKey, status| %>
			    		<%= render partial: "shared/historyCell", :locals => {:historyValue => status} %>
					<% end %>
					<td><%=link_to "Show", cdisc_cl_path(:id => cl.id, :namespace => cl.namespace ), :class => "btn btn-primary btn-xs" %></td>
					<td><%=link_to "Compare", compare_cdisc_cls_path(:id => cl.id, :oldTermId => @oldCdiscTerm.id, :oldTermNs => @oldCdiscTerm.namespace, :newTermId => @newCdiscTerm.id, :newTermNs => @newCdiscTerm.namespace, :type => "ALL"), :class => "btn btn-primary btn-xs" %></td>
				</tr>	
			<% end %>
		</tbody>
	</table>
</div>
