<h3>Changes: <%= @identifier %> <small>(CDISC, <%= @identifier %>)</small></h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
	  		<tr>
				<th>Code List</th>
				<th>Submission Value</th>
				<% clEntry = @Results.values[0] %>
				<% result = clEntry[:result]%>
				<% result.each do |vKey, status| %>
		    		<th><%= vKey %></th>
		  		<% end %>
				<th></th>
			</tr>
		</thead>
		<tbody>	
			<% @Results.each do |clKey, clEntry| %>
				<% result = clEntry[:result]%>
				<% identifier = clEntry[:identifier]%>
				<% clId = clEntry[:id]%>
				<tr>
					<td><%= clKey %></td>
					<td><%= identifier %></td>
					<% result.each do |vKey, status| %>
						<%= render partial: "shared/historyCell", :locals => {:historyValue => status} %>
					<% end %>
					<td><%=link_to "Changes", changes_cdisc_cls_path(:id => clId), :class => "btn btn-info btn-xs" %></td>
				</tr>	
			<% end %>
		</tbody>
	</table>
</div>
