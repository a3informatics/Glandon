<%= javascript_include_tag 'cdisc_cross_ref_panel', 'cdisc_cross_ref'%>
<%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Change Instructions:", :item => @cdisc_term} %>
<div class="col-md-12">
	<table id="cr_table" class="table table-striped table-bordered table-condensed">
	  <thead>
		  <tr>
		    <th class="col-sm-1">Parent Item</th>
		    <th class="col-sm-1">Item</th>
		    <th class="col-sm-2">Submission Value</th>
		    <th>Comments</th>
		    <th class="col-sm-3">References</th>
		  </tr>
		</thead>
		<tbody>
		</tbody>
	</table>
	<%= render partial: "shared/back_button" %>
	<input type="hidden" id="cr_direction" value="<%= @direction %>">
	<input type="hidden" id="cr_id" value="<%= @cdisc_term.id %>">
	<input type="hidden" id="cr_namespace" value="<%= @cdisc_term.namespace %>">
</div>

