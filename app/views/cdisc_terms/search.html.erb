<%= stylesheet_link_tag    'thesauri_search_new', 'spinner' %>
<%= javascript_include_tag 'thesauri_search_new', 'spinner' %>
<% second_level_breadcrumb("CDISC Terminology", history_cdisc_terms_path, "Search") %>
<% if Rails.env == "test" || Rails.env == "development" %>
	<%= javascript_include_tag 'rspec_helper' %>
<% end %>
<style>
	#searchTable { table-layout: fixed; }
</style>
<%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Search:", :item => @cdiscTerm} %>
<div class="col-md-12">
	<table id="searchTable" class="table table-striped table-bordered table-condensed">
  	<thead>
			<tr>
		    <th>Code List</th>
		    <th>Item</th>
		    <th>Notation</th>
		    <th>Preferred Term</th>
		    <th>Synonym</th>
		    <th>Definition</th>
	  	</tr>
		</thead>
		<tbody>
		</tbody>
		<tfoot>
      <tr>
		    <th>CL</th>
		    <th>Item</th>
		    <th>Notation</th>
		    <th>Preferred Term</th>
		    <th>Synonym</th>
		    <th>Definition</th>
      </tr>
  	</tfoot>
	</table>
	<%= link_to "Close", history_cdisc_terms_path, :class => "btn btn-primary" %>
</div>
</div>
<input type="hidden" id="thesaurus_current" value="<%= @cdiscTerm.current? %>">
<input type="hidden" id="thesaurus_single" value="<%= true %>">
<input type="hidden" id="thesaurus_id" value="<%= @cdiscTerm.id %>">
<input type="hidden" id="thesaurus_namespace" value="<%= @cdiscTerm.namespace %>" %>	