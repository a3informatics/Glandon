<h3>CDISC Terminology</h3>
<div class="col-md-9">
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
	  		<tr>
		    	<th>Internal Version</th>
		    	<th>Version Label</th>
		    	<th></th>
	  		</tr>
	  	</thead>
	  	<tbody>
	  		<% @cdiscTerms.each do |cdiscTerm| %>
			  	<tr>
			    	<td><%= cdiscTerm.version %></td>
			    	<td><%= cdiscTerm.versionLabel %></td>
			    	<td><%= link_to "Show", cdiscTerm, :class => "btn btn-primary btn-xs" %></td>
			  	</tr>
			<% end %>	
	  	</tbody>
	</table>
	<%= link_to "New version", new_cdisc_term_path, :class => "btn btn-success" %>
</div>
<div class="col-md-3">
	<div class="panel panel-default">
		<div class="panel-heading">Full History</div>
		<div class="panel-body">
			<%= link_to "History", history_cdisc_terms_path() , :class => "btn btn-info" %>
		</div>
	</div>
</div>

