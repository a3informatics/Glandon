<h1>CDISC Terminology</h1>
<div class="col-md-7">
	<h2>All Versions</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Internal Version</th>
	    <th>Version Label</th>
	    <th></th>
	  </tr>
		<% @cdiscTerms.each do |cdiscTerm| %>
		  <tr>
		    <td><%= cdiscTerm.version %></td>
		    <td><%= cdiscTerm.versionLabel %></td>
		    <td><%= link_to "Show", cdiscTerm, :class => "btn btn-primary" %></td>
		  </tr>
		<% end %>
	</table>
	<%= link_to "New version", new_cdisc_term_path, :class => "btn btn-success" %>
</div>
<div class="col-md-3">
	<div class="panel panel-default">
		<div class="panel-heading">Full History</div>
		<div class="panel-body">
			<%= link_to "History", history_cdisc_terms_path() , :class => "btn btn-info" %>
		</div>
	</div>
</div>

