<h3>Index: Domains</h3>
<div class="col-md-6">
		<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
		    	<th>Identifier</th>
		    	<th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @domains.each do |identifier| %>
				<tr>
			    	<td><%= identifier %></td>
			    	<td><%= link_to "History", history_domains_path(:identifier => identifier), :class => "btn btn-primary btn-xs" %></td>
			  	</tr>
			<% end %>
		</tbody>
	</table>
</div>