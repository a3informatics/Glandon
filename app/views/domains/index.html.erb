<h3>Domains</h3>
<div class="col-md-12">
	<table id="main" class="table table-striped table-bordered table-condensed">
	  	<thead>
	  		<tr>
	    		<th>Identifer</th>
	    		<th>Label</th>
	    		<th>Owner</th>
	    		<th>Internal Version</th>
	    		<th>Version</th>
	    		<th></th>
	  		</tr>
		</thead>
		<tbody>
	  		<% @domains.each do |key, domain| %>
	    		<tr>
	    			<td><%= domain.identifier %></td>
				    <td><%= domain.label %></td>
				    <td><%= domain.owner %></td>
				    <td><%= domain.version %></td>
				    <td><%= domain.versionLabel %></td>
				    <td><%= link_to "Show", domain_path(:id => domain.id, :namespace => domain.namespace), :class => "btn btn-primary btn-xs" %></td>
	  			</tr>
	  		<% end %>
		</tbody>
	</table>
</div>
