<% content_for :page_title do %> Import CDISC Terminology <% end %>

<% first_level_breadcrumb (:imports) %>
<% type = @file_type == 0 ? "Excel" : "API" %>

<div class="row">
	<%= render layout: "shared/headers/header_generic", :locals => {badge: "icon-import", header_title: "",
		main_text: "Import Centre", sub_texts: "CDISC Terminology Import" } do %>

			<%= render partial: "shared/buttons/return", :locals => { url: list_imports_path } %>

	<% end %>
</div>

<div class="row centered">
    <%= render partial: "shared/import/import_cdisc_terms_panel", locals: {create_path: imports_cdisc_terms_path, title: "Import CDISC Terminology using #{type}", multiple: true} %>
</div>
