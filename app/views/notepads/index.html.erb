<h3>Index: Notepad</h3>
	<div class="row">
		<div class="col-md-6">
			<h4>Code List Items</h4>
			<table id="main" class="table table-striped table-bordered table-condensed">
			  	<thead>
					<tr>
				  		<th>Identifier</th>
				    	<th>Submission Value</th>
				    	<th></th>

				  	</tr>
				</thead>
				<tbody>
					<% @items.each do |item| %>
					  	<tr>
					    	<td><%= item.identifier %></td>
						    <td><%= item.useful_1 %></td>
				    		<td><%= link_to "Delete", notepad_path(item.id), method: :delete, data: { confirm: "Are you sure?" }, :class => "btn btn-danger btn-xs" %></td>
				  		</tr>
					<% end %>
				</tbody>
			</table>
			<td><%= link_to "Delete All", destroy_term_notepads_path(), method: :delete, data: { confirm: "Are you sure?" }, :class => "btn btn-danger" %></td>
		</div>
	</div>
</div>
