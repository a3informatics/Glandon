<h1>Biomedical Concept Template <%= @bct.name %> <small>(CDISC, <%= @bct.identifier %>, V<%= @bct.version %>)</small></h1>
<div class="col-md-10">
	<h2>Details</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Identifer</th>
	    <th>Name</th>
	    <th>Version</th>
	  </tr>
	  <tr>
	    <td><%= @bct.identifier %></td>
	    <td><%= @bct.name %></td>
	    <td><%= @bct.version %></td>
	  </tr>
	</table>
	<h2>Items</h2>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
		<% aliasEntry = @bct.properties.values[0] %>
		<% aliasEntry.each do |key, value| %>
    		<th><%= key %></th>
		<% end %>
		<% @bct.properties.each do |key, aliasEntry| %>
			<tr>
				<% aliasEntry.each do |key, value| %>
					<td><%= value %></td>
				<% end %>
			</tr>
		<% end %>
	</table>
</div>
