<%= render partial: "shared/stdInstanceTitle", :locals => {:title => "Show:", :item => @bct} %>
<div class="col-md-12">
	<h4>Details</h4>
	<table class="table table-striped table-bordered table-condensed">
	  <tr>
	    <th>Identifer</th>
	    <th>Label</th>
	    <th>Owner</th>
	    <th>Version</th>
	    <th>Version Label</th>
	  </tr>
	  <tr>
	    <td><%= @bct.identifier %></td>
	    <td><%= @bct.label %></td>
	    <td><%= @bct.owner %></td>
	    <td><%= @bct.version %></td>
	    <td><%= @bct.versionLabel %></td>
	  </tr>
	</table>
	<h4>Items</h4>
	<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
	  	<tr>
				<th>Alias</th>
				<th>Question Text</th>
				<th>Prompt Text</th> 
				<th>Enabled</th> 
				<th>Collect</th> 
				<th>Datatype</th> 
			</tr>
		</thead>
		<tbody>
			<% @items.each do |key, item| %>
				<% if item.enabled %>
					<tr class="success">
				<% else %>
					<tr>
				<% end %>		
				<td><%= item.alias %></td>
				<td><%= item.qText %></td>
				<td><%= item.pText %></td>
				<td><%= item.enabled %></td>
				<td><%= item.collect %></td>
				<td><%= item.datatype %></td>
				</tr>
			<% end %>
		</tbody>
