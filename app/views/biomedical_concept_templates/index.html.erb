<h3>Index: Biomedical Concept Templates</h3>
<div class="col-md-6">
		<table id="main" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
		    	<th>Identifier</th>
		    	<th>Label</th>
		    	<th></th>
		  	</tr>
		</thead>
		<tbody>
			<% @bcts.each do |item| %>
				<tr>
			    	<td><%= item[:identifier] %></td>
			    	<td><%= item[:label] %></td>
			    	<td><%= link_to "History", history_biomedical_concept_templates_path(:identifier => item[:identifier]), :class => "btn btn-primary btn-xs" %></td>
			  	</tr>
			<% end %>
		</tbody>
	</table>
</div>
