<%= javascript_tag 'setPageTitle("List Change notes: '+@managed_item.label+'")' %>
<% third_level_breadcrumb(:code_lists, @managed_item, @close_path) %>

<%= stylesheet_link_tag 'shared/annotations', 'shared/datatables_loading' %>
<%= javascript_include_tag 'iso_managed/list_change_notes', 'shared/list_change_notes_panel' %>

<%= javascript_tag "var dataUrl = '#{list_change_notes_data_iso_managed_v2_path(@managed_item)}'" %>

<div class="row">
  <%= render layout: "shared/headers/header_managed_item", :locals => {:item => @managed_item, :badge => get_iso_managed_icon(@managed_item),
    :header_title => "List Change notes" } do %>
    <div class="actions-wrap">
      <%= render partial: "shared/buttons/ico_btn_secondary", :locals => {link_path: export_change_notes_csv_iso_managed_v2_path(@managed_item), id: "export-csv",
        icon: "icon-file", btn_text: "Export List (CSV)", dt_toggle: "", disabled: false } %>
			<%= render partial: "shared/buttons/ico_btn_secondary", :locals => {link_path: @close_path, id: "close",
				icon: "icon-long-arrow", btn_text: "Return", dt_toggle: "", disabled: false } %>
    </div>
  <% end %>
</div>
<div class="row">
	<div class="col-md-12 card wide table-cardstyle all-columns">
		<div class="card-header">	Change Notes of <%= @managed_item.has_identifier.identifier %> and Children</div>
	  <div class="card-content">
      <table id="list-change-notes-table" class="table table-striped table-bordered table-condensed">
        <thead>
          <tr>
            <th class="fit">Identifier</th>
            <th class="fit">Submission Value</th>
            <th>Label</th>
            <th><span class="icon-note text-link"></span> Timestamp</th>
            <th><span class="icon-note text-link"></span> User Reference</th>
            <th><span class="icon-note text-link"></span> Reference</th>
            <th><span class="icon-note text-link"></span> Description</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
	</div>
</div>
