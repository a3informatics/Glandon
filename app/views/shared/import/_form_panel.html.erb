<%= javascript_include_tag 'standard_datatable' %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Forms</h3>
  </div>
  <div class="panel-body">
    <table id="main" class="table table-striped table-bordered table-condensed">
      <thead>
          <tr>
            <th>Identifier</th>
            <th>Label</th>
            <th></th>
          </tr>
      </thead>
        <% @forms.each do |item| %>
          <tr>
            <td><%= item[:identifier] %></td>
            <td><%= item[:label] %></td>
            <td><%= link_to "Import", import_path.call(imports: {identifier: item[:identifier], filename: item[:filename]}), :class => "btn btn-success btn-xs", method: :post %></td>
          </tr>
        <% end %>
      <tbody>
      </tbody>
    </table>
  </div>
</div>