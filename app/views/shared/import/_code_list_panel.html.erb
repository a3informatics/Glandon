<%= stylesheet_link_tag    'spinner' %>
<%= javascript_include_tag 'spinner' %>
<style>
  #list_table_processing.panel-default {
    border: 1px solid transparent;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0);
  }
  #error_table_processing.panel-default {
    border: 1px solid transparent;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0);
  }
</style>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Code Lists</h3>
  </div>
  <div class="panel-body">
    <div id="list_table_div">
      <h4>Code Lists:</h4>
      <table id="list_table" class="table table-striped table-bordered table-condensed">
        <thead>
            <tr>
              <th class="col-md-2">Identifier</th>
              <th>Label</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
    <div class="btn-group" role="group" aria-label="First group">
      <button id="import_spinner" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-refresh"></span>
      </button>
      <button type="button" id="import_button" class="btn btn-success">Import</button>
    </div>
    <div id="error_table_div">
      <h4>Errors:</h4>
      <table id="error_table" class="table table-striped table-bordered table-condensed">
        <thead>
            <tr>
              <th class="col-md-1">Index</th>
              <th>Error Message</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>