<%= javascript_include_tag 'shared/thesauri/differences_panel', 'shared/thesauri/changes_panel' %>
<style>
  <%= Diffy::CSS %>
</style>
<div class="text-xnormal text-link ci-card-title">
  Differences summary
</div>
<table id="differences_table" class="table table-bordered table-striped table-condensed">
  <thead>
    <tr>
      <th>Date</th>
      <th class="fit">Identifier</th>
      <th class="fit">Submission Value</th>
      <th>Preferred Term</th>
      <th>Synonym</th>
      <th>Definition</th>
    </tr>
  <thead>
</table>
<div class="text-xnormal text-link ci-card-title">
  Changes summary
</div>
<table id="changes" class="table table-bordered table-striped table-condensed">
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Label</th>
      <th>Submission Value</th>
      <th class="text-center">&nbsp;</th>
      <th class="text-center">&nbsp;</th>
      <th class="fit"></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
