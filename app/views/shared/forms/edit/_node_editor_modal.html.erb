<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="tableModalLabel" aria-hidden="true" id="node-editor">
  <div class="modal-dialog modal-lg medium-screen" role="document">

    <div class="modal-content">

      <div class="modal-header">
        <span class="modal-title text-normal text-medium" id="tableModalLabel">
          <span class="icon-edit text-link text-small" style="margin-right: 5px;"></span>
          Edit Node
        </span>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body font-light">

        <div class="error-modal" style="width: 100%"></div>

        <div class="ne-content form-wrap"></div>

      </div>

      <div class="modal-footer">
        <button type="button" class="btn medium light" id="editor-close" data-dismiss="modal" aria-label="Close">Close</button>
        <button type="button" class="btn medium green" id="editor-submit">Save changes</button>
      </div>

    </div>
  </div>
</div>
