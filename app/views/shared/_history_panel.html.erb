<%= javascript_include_tag 'shared/history_panel', 'shared/tags' %>
<%= stylesheet_link_tag 'custom/context_menu', media: 'all' %>
<%= javascript_include_tag 'shared/context_menu' %>
<%= stylesheet_link_tag 'shared/datatables_loading' %>
<% if Rails.env == "test" || Rails.env == "development" %>
  <%= javascript_include_tag 'rspec_helper' %>
<% end %>
<h3 class="card-header"><%= title %></h3>
<div class="card-content">
  <table id="history" class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        <th class="fit">Version</th>
        <th class="fit">Last Change Date</th>
        <th>Owner</th>
        <th>Identifier</th>
        <th>Label</th>
        <th>Version Label</th>
        <th>State</th>
        <th class="fit ignore"></th>
        <th class="fit"></th>
      </tr>
    </thead>
  </table>
</div>
