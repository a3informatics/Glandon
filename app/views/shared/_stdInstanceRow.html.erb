<tr>
	<td><%= item.version %></td>
	<td><%= item.label %></td>
	<td><%= item.identifier %></td>
	<td><%= item.versionLabel %></td>
	<td><%= item.owner %></td>
	<td><%= link_to "Show", showPath, :class => "btn btn-primary btn-xs" %></td>
	<% if viewPath != "" %>
		<td><%= link_to "View", viewPath, :class => "btn btn-primary btn-xs" %></td>
	<% else %>
		<td></td>
	<% end %>
	<% if searchPath != "" %>
		<td><%= link_to "Search", searchPath, :class => "btn btn-primary btn-xs" %></td>
	<% else %>
		<td></td>
	<% end %>
	<% if editPath != "" and item.edit? and item.latest? %>
		<td><%= link_to "Edit", editPath, :class => "btn btn-primary btn-xs" %></td>
	<% else %>
		<td></td>
	<% end %>
	<% if item.registered? %>
		<td><%= item.registrationState.registrationStatus %></td>
		<% if current_item != nil %>
			<td><%= link_to "Status", status_iso_managed_news_index_path(:id => item.id, :namespace => item.namespace, :current_id => current_item.registrationState.id), :class => "btn btn-primary btn-xs" %></td>
		<% else %>
			<td><%= link_to "Status", status_iso_managed_news_index_path(:id => item.id, :namespace => item.namespace, :current_id => ""), :class => "btn btn-primary btn-xs" %></td>
		<% end %>
		<% if item.current? %>
			<td class="text-center bg-success">
				<span class="text-center glyphicon glyphicon-ok"/>
			</td>
		<% else %>
			<td></td>
		<% end %>
	<% else %>
		<td></td>
		<td></td>
		<td></td>
	<% end %>
	<% if deletePath != "" and item.delete? %>
		<td><%= link_to "Delete", deletePath, method: :delete, data: { confirm: "Are you sure?" }, :class => "btn btn-danger btn-xs" %></td>
	<% else %>
		<td></td>
	<% end %>
	<td><%= link_to "Triples", view_dashboard_index_path(:id => item.id, :namespace => item.namespace), :class => "btn btn-warning btn-xs" %></td>
</tr>