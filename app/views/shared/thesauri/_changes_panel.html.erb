<%= javascript_include_tag 'shared/thesauri/changes_panel' %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= title %></h3>
  </div>
  <div class="panel-body">
  	<table id="changes" class="table table-striped table-bordered table-condensed">
  	  <thead>
  			<tr>
  				<th>Identifier</th>
  				<th>Label</th>
  				<th>Submission Value</th>
  				<% (1..column_count).each do |result| %>
  			    <th class="text-center">&nbsp;</th>
  			  <% end %>
  				<th></th>
  			</tr>
  		</thead>
  		<tbody>	
  		</tbody>
  	</table>
    <%= render partial: "shared/forward_backward", :locals => {:links => links} %>
  </div>
</div>