<%= javascript_include_tag 'shared/thesauri/subsets_index' %>
<%= stylesheet_link_tag 'spinner', 'homes' %>
<% if Rails.env == "test" || Rails.env == "development" %>
  <%= javascript_include_tag 'rspec_helper' %>
<% end %>
              <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="tableModalLabel" aria-hidden="true" id="subsetsModal">
                <div class="modal-dialog modal-lg" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h4 class="modal-title" id="tableModalLabel">Subsets of <%= @tc.identifier %></h4>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                    <div class="modal-body">
                      <div class="row">
                        <div class="col-sm-12 table-cardstyle">
                          <table style="width: 100%;" id="ssIndexTable" class="table table-striped table-bordered table-condensed">
                            <thead>
                              <tr>
                                <th>Belongs to</th>
                                <th class="fit">Subset Identifier</th>
                                <th>Subset Label</th>
                                <th class="fit"></th>
                              </tr>
                            </thead>
                            <tbody>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                    <div class="modal-footer">
                      <button type="button" class="btn medium" data-dismiss="modal" aria-label="Close">Close</button>
                      <button type="button" class="btn green medium" id="new_subset">+ New subset</button>
                    </div>
                  </div>
                </div>
              </div>
