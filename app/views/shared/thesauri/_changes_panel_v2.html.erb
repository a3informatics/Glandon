<%= javascript_include_tag 'shared/thesauri/changes_panel' %>

<div class="card">
    <div class="card-header">
      <%= title %>
      <span class="icon-info-circle ttip text-small text-bright" style="margin-left: 4px;">
        <span class="ttip-text shadow-small text-dark"><%= if info.empty? then "" else info end %></span>
      </span>
    </div>
  <div class="card-content">
  	<table id="changes" class="table table-striped table-bordered table-condensed">
  	  <thead>
  			<tr>
  				<th>Identifier</th>
  				<th>Label</th>
  				<th>Submission Value</th>
  				<% (1..column_count).each do |result| %>
  			    <th class="text-center">&nbsp;</th>
  			  <% end %>
  				<th></th>
  			</tr>
  		</thead>
  		<tbody>
  		</tbody>
  	</table>
    <%= render partial: "shared/forward_backward", :locals => {:links => links} %>
  </div>
</div>
