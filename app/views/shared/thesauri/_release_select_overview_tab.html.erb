<div class="col-sm-12 table-cardstyle">
  <br/>
  <%= yield %>
  <br/>

  <table id="<%=id%>" class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        <th>Identifier</th>
        <th class="th-small">Submission Value</th>
        <th>Preferred Term</th>
        <th>Synonym(s)</th>
        <th class="th-small">Tags</th>
        <th>Version</th>
        <th>State</th>
        <th class="ignore fit"></th>
        <th class="ignore fit"></th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
  <button id="<%=id%>-bulk-deselect" class="btn light medium ttip">
    <span class="icon-times text-tiny"></span> Exclude all <span class="ttip-text ttip-top shadow-small text-medium text-small">This will only affect items filtered by search.</span>
  </button>
</div>
