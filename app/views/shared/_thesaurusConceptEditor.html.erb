<style>
  div.DTE_Inline.DTE_Processing:after {
      background: url('<%= asset_path('ajax-loader-small.gif') %>') no-repeat top left;
  }
</style>
<h4>Code Lists</h4>
<table id="editor_table" class="table table-striped table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr> 
      <th>Identifer</th>
      <th>Label</th>
      <th>Submission Value</th>
      <th>Preferred Term</th>
      <th>Synonym(s)</th>
      <th>Definition</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
<div class="row">
  <div class="col-md-4">
    <div id="formInfo" class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">New Code List Entry</h3>
      </div>
      <div class="panel-body">
        <form id="main_form">
          <div>
            <div class="form-group">
              <label for="tcIdentifier">Identifier:</label>
              <div class="input-group">
                <span class="input-group-addon" id="tcIdentifierPrefix"><%= @tc_identifier_prefix %></span>
                <input type="text" class="form-control" id="tcIdentifier" name="Code List Identifier">
              </div>
            </div>
          </div>
          <button type="submit" id="tc_new" class="btn btn-success">New</button>
        </form>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-4">
    <div class="btn-group" role="group" aria-label="First group">
      <button id="saving" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-refresh"></span>
      </button>
      <button id="token_timer_1" type="button" class="btn btn-success">00:00</button>
      <button type="submit" id="save" class="btn btn-primary">Save</button>
    </div>
    <button type="button" id="close_button" class="btn btn-primary">Close</button> 
  </div>
</div>