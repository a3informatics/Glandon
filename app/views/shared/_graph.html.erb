<div class="col-md-4">
  <br/>
  <div class="input-group">
    <span class="input-group-addon" id="sizing-addon1"><b>Type:</b></span>
    <input id="concept_type" type="text" class="form-control" aria-describedby="sizing-addon1">
  </div>
</div>
<div class="col-md-5">
  <br/>
  <div class="input-group">
    <span class="input-group-addon" id="sizing-addon2"><b>Label:</b></span>
    <input id="concept_label" type="text" class="form-control" aria-describedby="sizing-addon2">
  </div>
</div>
<div class="col-md-1">
</div>
<div class="col-md-2">
  <br/>
  <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
    <div class="btn-group" role="group" aria-label="First group">
      <button id="graph_running" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-refresh glyphicon-spin"></span>
      </button>
      <button id="graph_stop" type="button" class="btn btn-default btn-primary">
        <span class="glyphicon glyphicon-pause" aria-hidden="true"></span>
      </button>
      <button id="graph_focus" type="button" class="btn btn-default btn-primary">
        <span class="glyphicon glyphicon-zoom-in" aria-hidden="true"></span>
      </button>
    </div>
  </div>
</div>