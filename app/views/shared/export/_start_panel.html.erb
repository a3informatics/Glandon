<%= javascript_include_tag 'export/start_panel' %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Exports</h3>
  </div>
  <div class="panel-body">
    <div class="row text-center" id="spinner_div">
      <img src='<%= asset_path("loading.gif") %>'>
    </div>
    <div id="item_table_div">
      <table id="item_table" class="table table-striped table-bordered table-condensed">
        <thead>
          <tr>
            <th>Identifier</th>
            <th>Label</th>
            <th class="fit">Version</th>
            <th class="fit"></th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>
