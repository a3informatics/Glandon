<%= stylesheet_link_tag    'spinner' %>
<%= javascript_include_tag 'standard_datatable', 'spinner' %>
<style>
  #list_table_processing {
    border: none;
  } 
</style>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Code Lists</h3>
  </div>
  <div class="panel-body">
    <table id="list_table" class="table table-striped table-bordered table-condensed">
      <thead>
          <tr>
            <th class="col-md-2">Identifier</th>
            <th>Label</th>
          </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
    <div class="btn-group" role="group" aria-label="First group">
      <button id="import_spinner" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-refresh"></span>
      </button>
      <button type="button" id="import_button" class="btn btn-success">Import</button>
    </div>
  </div>
</div>