<% if !local_assigns[:children_url].nil? %>
  <%= javascript_tag "const childrenDataUrl = '#{children_url}'" %>
<% end %>

<% if local_assigns.fetch :custom_props?, false %>

  <%= javascript_tag "const customPropsOpts = {
                              dataUrl: '#{ custom_properties_iso_managed_v2_path(item) }',
                              accessPolicy: #{ policy(IsoManaged).show? }
                            }; " %>

<% end %>

<table id="<%= local_assigns.fetch :id, 'children' %>" class="table <%= local_assigns.fetch :table_classes, 'striped' %>">
  <thead>
    <tr>

      <% if !(local_assigns.fetch :first_column, '').empty? %>
        <th><%= local_assigns.fetch :first_column, '' %></th>
      <% end %>

      <th class="fit">Identifier</th>
      <th class="fit">Submission Value</th>
      <th>Preferred Term</th>
      <th>Synonyms</th>

      <% if local_assigns.fetch :extensible?, false %>
        <th class="fit">Extensible</th>
      <% end %>

      <th>Definition</th>
      <th>Tags</th>

      <%# Extra Columns %>
      <%= yield  %>

      <% if local_assigns.fetch :indicators?, true %>
        <th class="ignore fit"></th>
      <% end %>

      <%# Last Column Button area %>
      <% if local_assigns.fetch :btn_in_last?, false %>
        <th class="fit"></th>
      <% end %>

    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
