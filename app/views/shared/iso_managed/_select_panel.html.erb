<div class="card">
		<h3 class="card-header"><%= title %></h3>
	<div class="card-content">
		<div id="ims_list_table_div">
			<table id="ims_list_table" class="table table-striped table-bordered">
				<thead>
					<tr> 
						<th>Identifier</th>
						<th>Label</th>			                                
						<th>Version</th>			                                
					</tr>
				</thead>
				<tbody>
				</tbody>
			</table>
		</div>
		<div id="ims_all_table_div">
			<table id="ims_all_table" class="table table-striped table-bordered">
				<thead>
					<tr> 
						<th>Identifier</th>
						<th>Label</th>			                                
						<th>Version</th>			                                
					</tr>
				</thead>
				<tbody>
				</tbody>
			</table>
		</div>
		<div class="btn-group" role="group">
			<button type="button" id="ims_add_button" class="btn " data-toggle="tooltip" data-placement="bottom" title="Add item">
				<span class="glyphicon glyphicon-plus"></span>
			</button>
			<button type="button" id="ims_list_all_button" class="btn btn-success" data-toggle="tooltip" data-placement="bottom" title="Change source of items.">
				<span class="glyphicon glyphicon-thumbs-up"></span>
			</button>
		</div>
		<input type="hidden" id="ims_list_path" value="<%= list_path %>">
		<input type="hidden" id="ims_all_path" value="<%=  all_path %>">
	</div>
</div>
