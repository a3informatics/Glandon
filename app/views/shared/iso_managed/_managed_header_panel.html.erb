<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= instance_title(title, item) %></h3>
  </div>
  <div class="panel-body">
    <table id="header_table" class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>Identifier</th>
          <th>Label</th>
          <th>Owner</th>
          <th>Status</th>
          <th>Version</th>
          <th>Version Label</th>
          <th class="fit"></th>
        </tr>
      </thead>
        <tbody>
          <tr>
            <td><%= item.identifier %></td>
            <td><%= item.label %></td>
            <td><%= item.owner_short_name %></td>
            <td><%= item.registration_status %></td>
            <td><%= item.semantic_version %></td>
            <td><%= item.version_label %></td>
            <td>
              <%= link_to "Impact", impact_thesauri_index_path(id: item.uri.fragment, namespace: item.uri.namespace), :class => "btn btn-info btn-xs" %>
            </td>
          </tr>
      </tbody>
    </table>
  </div>
</div>