<%= javascript_include_tag 'shared/iso_managed/comments_panel' %>
<%= stylesheet_link_tag 'shared/datatables_loading' %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Comments By Version</h3>
  </div>
  <div class="panel-body">
    <table id="comments_table" class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th class="fit">Version</th>
          <th class="fit">Creation Date</th>
          <th class="fit">Last Change Date</th>
          <th>Description</th>
          <th>Comments</th>
          <th>References</th>
          <th class="fit"></th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
  </div>
</div>