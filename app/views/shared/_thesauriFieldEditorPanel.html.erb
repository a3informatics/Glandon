<div class="panel panel-default">
  <div class="panel-heading">
    <h3 id="tfe_panel_title" class="panel-title">Terminology</h3>
    <span class="pull-right clickable"><i class="glyphicon glyphicon-chevron-up"></i></span>
  </div>
  <div class="panel-body">
    <table id="tfe_item_table" class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>Item</th>
          <th>Notation</th>
          <th>Preferred Term</th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
    <div class="btn-group" role="group">
      <button id="tfe_processing" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-refresh"></span>
      </button>
      <button id="tfe_add_item" class="btn btn-primary">Add</button>
      <button id="tfe_delete_item" class="btn btn-danger">Delete</button>
      <button id="tfe_delete_all_items" class="btn btn-danger">Delete All</button>
    </div>
  </div>
</div>