<h3>New Registration Authority</h3>
<div class="col-md-3">
	<%= form_for @registrationAuthority do |f| %>
		<div class="form-group">
		  <%= f.label :number, "DUNS Number:" %><br />
		  <%= f.text_field :number, autofocus: true, class: "form-control" %>
		</div>
		<div class="form-group">
		  	<label for="name">Organisation (Namespace):</label>
            <%= select_tag("registration_authority[namespaceId]", options_for_select(@namespaces)) %>
        </div>
        <%= f.submit "Submit", class: "btn btn-default btn-primary" %>  
	<% end %>
</div>