<h3>New Owner</h3>
<div class="col-md-3">
	<%= form_for @registrationAuthority do |f| %>
	  <div class="form-group">
	    <%= f.label :number, "Number:" %><br />
	    <%= f.text_field :number, autofocus: true, class: "form-control" %>
	  </div>
	  <div class="form-group">
	    <%= f.label :shortName, "Short Name:"%><br />
	    <%= f.text_field :shortName, class: "form-control" %>
	  </div>
	  <div class="form-group">
	    <%= f.label :name, "Long Name:" %><br />
	    <%= f.text_field :name, class: "form-control" %>
	  </div>	  
	  <%= f.submit "Submit", class: "btn btn-default btn-primary" %>  
	<% end %>
</div>