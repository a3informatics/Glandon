<% second_level_breadcrumb("Namespaces", iso_namespaces_path, "New") %>
<h3>New Namespace</h3>
<div class="col-md-3">
	<%= form_for @namespace do |f| %>
	   <div class="form-group">
	    <%= f.label :shortName, "Short Name:"%><br />
	    <%= f.text_field :shortName, autofocus: true , class: "form-control"%>
	  </div>
	  <div class="form-group">
	    <%= f.label :name, "Name:"%><br />
	    <%= f.text_field :name, class: "form-control" %>
	  </div>
	  <%= f.submit "Submit", class: "btn btn-default btn-primary" %>  
	<% end %>
</div>

