<BiomedicalConceptTemplates>
	<BiomedicalConceptTemplate Id="Obs PQR" Name="Simple Observation PQR Biomedical Research Concept Template" >
		<Class Name="DefinedActivity">
			<Attribute Alias="Category" Name="categoryCode">
				<Property Alias="Category (--CAT)" Name="code"/>
			</Attribute>
			<Attribute Alias="Subcategory" Name="subcategoryCode">
				<Property Alias="Subcategory (--SCAT)" Name="code"/>
			</Attribute>
		</Class>
		<Class Name="DefinedObservation">
			<Attribute Alias="Test" Name="nameCode">
				<Property Alias="Test Code (--TESTCD)" Name="code"/>
				<Property Alias="Name" Name="originalText">
					<Property Alias="Test Name (--TEST)" Name="value"/>
				</Property>
			</Attribute>
			<!--<Attribute Alias="approachSite" Name="approachAnatomicSiteCode">
				<Property Alias="Approach Site (TBD)" Name="code"/>
			</Attribute>-->
			<Attribute Alias="SiteOfAdministration" Name="targetAnatomicSiteCode">
				<Property Alias="Site Of Administration (--LOC)" Name="code"/> 
			</Attribute>
			<Attribute Alias="Method" Name="methodCode">
				<Property Alias="Method (--METHOD)" Name="code"/> 			
			</Attribute>
		</Class>
		<Class Name="PerformedObservation"> 
			<Attribute Alias="Position" Name="bodyPositionCode">
				<Property Alias="Body Position (--POS)" Name="code"/>
			</Attribute>
			<Attribute Alias="Laterality" Name="targetAnatomicSiteLateralityCode">
				<Property Alias="Laterality (--LAT)" Name="code"/>
			</Attribute>
			<Attribute Alias="DateTime" Name="dateRange">
				<Property Alias="Date" Name="low">
					<Property Alias="Date Time (--DTC)" Name="value"></Property>
				</Property>
			</Attribute>
			<Attribute Alias="ReasonNotDone" Name="negationReason">
				<Property Alias="Code"  Name="code">
					<Property Alias="Reason Not Done (--REASND)" Name="code"/>
				</Property>
			</Attribute>
		</Class>
		<Class Name="PerformedClinicalResult"> 
			<Attribute Alias="Result" Name="value" Datatype="PQR">
				<Property Alias="Result Value (--ORRES)"  Name="value"/>
				<Property Alias="Result Units (--ORRESU)"  Name="code"/>
			</Attribute>
			<Attribute Alias="Baseline" Name="baselineIndicator">
				<Property Alias="Baseline (--BLFL)"  Name="value"/>
			</Attribute>
		</Class>
	</BiomedicalConceptTemplate>
