<BiomedicalConceptTemplates Version="1">
	<BiomedicalConceptTemplate Id="BCT CM" Name="Concomitant Medications Biomedical Concept Template">
		<Class Name="Product">
			<Attribute Alias="Result" Name="description" Datatype="ST">
				<Property Alias="Name (--TRT)"  Name="value"/>
			</Attribute>
			<Attribute Alias="Form" Name="formCode">
				<Property Alias="Dose Form (--DOSFRM)" Name="code"/>
			</Attribute>
		</Class>
		<Class Name="DefinedObservation">
			<Attribute Alias="Category" Name="categoryCode">
				<Property Alias="Category (--CAT)" Name="code"/>
			</Attribute>
			<Attribute Alias="Subcategory" Name="subcategoryCode">
				<Property Alias="Subcategory (--SCAT)" Name="code"/>
			</Attribute>
		</Class>
		<Class Name="PerformedSubstanceAdministration"> 
			<Attribute Alias="Not Done" Name="negationIndicator">
				<Property Alias="Not Done (--STAT)" Name="value"/>
			</Attribute>
			<Attribute Alias="Reason Not Done" Name="negationReason">
				<Property Alias="Code"  Name="code">
					<Property Alias="Reason Not Done (--REASND)" Name="code"/>
				</Property>
			</Attribute>
			<Attribute Alias="Dose" Name="activeIngredientDose">
				<Property Alias="Dose Value (--DOSE)"  Name="value"/>
				<Property Alias="Dose Units (--DOSU)"  Name="unit"/>
			</Attribute>
			<Attribute Alias="Amount" Name="activeIngredientDoseDescription">
				<Property Alias="Description (--DOSTXT)" Name="value"/>
			</Attribute>
			<Attribute Alias="Frequency" Name="doseFrequencyCode">
				<Property Alias="Dose Frequency (--DOSFRQ)" Name="code"/>
			</Attribute>
			<Attribute Alias="Regimen" Name="productDoseDescription">
				<Property Alias="Dose Regimen (--DOSRGM)" Name="value"/>
			</Attribute>
			<Attribute Alias="Route" Name="routeOfAdministrationCode">
				<Property Alias="Route of Administration (--ROUTE)" Name="code"/>
			</Attribute>
			<Attribute Alias="Dates" Name="dateRange">
				<Property Alias="Start DateTime" Name="low">
					<Property Alias="Start DateTime (--STDTC)" Name="value"/>
				</Property>
				<Property Alias="End DateTime" Name="high">
					<Property Alias="End DateTime (--ENDTC)" Name="value"/>
				</Property>
			</Attribute>
		</Class>		
	</BiomedicalConceptTemplate>
</BiomedicalConceptTemplates>

