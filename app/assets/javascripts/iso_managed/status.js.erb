$(document).ready(function() {
  
$('#select_release').on('change', function() {
  var value = $(this).val();
  console.log(url_release);
  $.ajax({
    url: url_release,
    type: "PUT",
    data: { "iso_managed": { "sv_type": value }},
    dataType: 'json',
    error: function (xhr, status, error) {
      var html = alertError("An error has occurred updating release.");
      displayAlerts(html);
    },
    success: function(result){
      location_reload;
    }
  });

});

});