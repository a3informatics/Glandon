$(document).ready( function() {
  var shp = new ShowHeaderPanel(is_extended, is_extending);
  var modalTS = new ThesauriSelect(tc_id, shp.callback);
  var modalSubsets = new IndexSubsets(tc_id);
  var columns = [
    {"data" : "identifier"},
    {"data" : "notation"},
    {"data" : "preferred_term"},
    {"data" : "synonym"},
    {"data" : "definition"},
    {"data" : "tags"}
  ];
  var cp = new ChildrenPanel(url_path, 1000, columns, tc_id, policy_edit, edit_enabled, editCallback);
  var searchModal = new TermSearch(context_id, cp.addNew.bind(cp));
  var timer = new Timer($('#edit_lock_token').val(), "imh_header", $('#warning_timeout').val());
  var unload = new UnloadV2($('#edit_lock_token').val(), false, unloadCallback);

  function unloadCallback() {
    timer.unload();
  }

  function editCallback() {
    timer.extendLock();
  }

  $("#new_subset").click(function(){
    modalTS.setCallback(modalSubsets.tsCallback);
    modalTS.resetUi();
  });
  
});
