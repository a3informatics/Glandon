$(document).ready( function() {
  var shp = new ShowHeaderPanel(is_extended, is_extending);
  var modalTS = new ThesauriSelect(tc_id, shp.callback);
  var modalSubsets = new IndexSubsets(tc_id);
  var columns = [
    {"data" : "identifier"},
    {"data" : "notation"},
    {"data" : "preferred_term"},
    {"data" : "synonym"},
    {"data" : "definition"}
  ];
  var cp = new ChildrenPanel(url_path, 1000, columns, tc_id, policy_edit, edit_enabled);
  var searchModal = new TermSearch(context_id, cp.addNew.bind(cp));

  $("#extend").click(function(){
    modalTS.setCallback(shp.callback);
    modalTS.resetUi();
  });
  $("#new_subset").click(function(){
    modalTS.setCallback(modalSubsets.tsCallback);
    modalTS.resetUi();
  });
});
