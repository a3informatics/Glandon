$(document).ready( function() {
  var subsetEditPanel = new SubsetEditChildrenPanel(subset_children_url, 1000, loadCallback);
  var sourceChildrenPanel = new SubsetSourceChildrenPanel(source_children_url, 1000, subsetEditPanel, loadCallback);

  function loadCallback(obj){
    obj.isLoaded = true;
    if(subsetEditPanel.isLoaded && sourceChildrenPanel.isLoaded){
      subsetEditPanel.markExistingItems();
    }
  }
});
