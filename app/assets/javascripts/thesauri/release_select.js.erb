var tl_slider;

var tabSwitchCallback = function(tab){
  switch($(tab).attr("id")){
    case "tab-cdisc-cls":
      if(tl_slider == null){
        tl_slider = new TimelineSlider($(".timeline-container"), $("#timeline-start-btn"), null, $(".timeline-point"));
        var newest_date = tl_slider.tl_points.eq(-1).children().first().html();
        tl_slider.moveToDate(tl_slider.l_slider, newest_date);
      }
      break;
    default:
      break;
  }
}

