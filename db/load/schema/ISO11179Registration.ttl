# baseURI: http://www.assero.co.uk/ISO11179Registration
# imports: http://www.assero.co.uk/ISO11179Identification
# imports: http://www.w3.org/2004/02/skos/core

@prefix : <http://www.assero.co.uk/ISO11179Registration#> .
@prefix isoI: <http://www.assero.co.uk/ISO11179Identification#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/ISO11179Registration>
  rdf:type owl:Ontology ;
  owl:versionInfo "v3.2.0"^^xsd:string ;
  rdfs:label "ISO11179 Registration Schema"^^xsd:string ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
:RegistrationAuthority
  rdf:type owl:Class ;
  rdfs:label "Registration authority"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "The organisation responsible for the registry who shall determine which metadata items should become IdentifiedItems, RegisteredItems, Designatable_Items and/or ClassifiableItems."^^xsd:string ;
.
:internationalCodeDesignator
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationAuthority ;
  rdfs:label "International Code Designator"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The scheme used to identify this authorty."^^xsd:string ;
.
:organizationIdentifier
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationAuthority ;
  rdfs:label "Organization Identifier"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The identifier under the scheme used to identify the authority."^^xsd:string ;
.
:owner
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Repository owner."^^xsd:string ;
  rdfs:domain :RegistrationAuthority ;
  rdfs:range xsd:boolean ;
  skos:definition "Defines the RA that is owner of the repository."^^xsd:string ;
.
:RegistrationState
  rdf:type owl:Class ;
  rdfs:label "Registration"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "The state of an item registered within the repository."^^xsd:string ;
.
:raNamespace
  rdf:type owl:ObjectProperty ;
  rdfs:domain :RegistrationAuthority ;
  rdfs:label "Scope Relationship"^^xsd:string ;
  rdfs:range isoI:Namespace ;
  skos:definition "The namespace used by the authrity to uniquely identifier items within the authorities scope."^^xsd:string ;
.
:byAuthority
  rdf:type owl:ObjectProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Authority Relationship"^^xsd:string ;
  rdfs:range :RegistrationAuthority ;
  skos:definition "The authority managing the registration status.."^^xsd:string ;
.
:registrationStatus
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Registration Status"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The current state of the registration"^^xsd:string ;
.
:effectiveDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Effective Date"^^xsd:string ;
  rdfs:range xsd:dateTime ;
  skos:definition "The date time from which the item is effective and can be used."^^xsd:string ;
.
:untilDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Until Date"^^xsd:string ;
  rdfs:range xsd:dateTime ;
  skos:definition "The date time until which the item is effective and can be used."^^xsd:string ;
.
:administrativeNote
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Administrative NoteÂ§"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Any notes regarding the registration status."^^xsd:string ;
.
:unresolvedIssue
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Unresolved Issue"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Any unresolved issues regarding the registration."^^xsd:string ;
.
:administrativeStatus
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Administrative Status"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The administrative status"^^xsd:string ;
.
:previousState
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Previous State"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The previous registration state."^^xsd:string ;
.
:multipleEdit
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :RegistrationState ;
  rdfs:label "Multiple Edit"^^xsd:string ;
  rdfs:range xsd:boolean ;
  skos:definition "Allows for multiple edit without new copy and version."^^xsd:string ;
