# baseURI: http://www.assero.co.uk/BusinessDomain

@prefix : <http://www.assero.co.uk/BusinessDomain#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix bo: <http://www.assero.co.uk/BusinessOperational#> .
@prefix cbc: <http://www.assero.co.uk/CDISCBiomedicalConcept#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/BusinessDomain>
  rdf:type owl:Ontology ;
  owl:imports <http://www.assero.co.uk/BusinessOperational> ;
  owl:imports <http://www.assero.co.uk/CDISCBiomedicalConcept> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:Item
  rdf:type owl:Class ;
  rdfs:label "Item"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Lowest level building block within the area."^^xsd:string ;
.
:Column
  rdf:type owl:Class ;
  rdfs:label "Item"^^xsd:string ;
  rdfs:subClassOf :Item ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Column within a tabulation."^^xsd:string ;
.
:Tabulation
  rdf:type owl:Class ;
  rdfs:label "Tabulation"^^xsd:string ;
  rdfs:subClassOf :Item ;
  rdfs:subClassOf bo:Component ;
  skos:definition "A tabulation. A set of columns."^^xsd:string ;
.
:ModelVariable
  rdf:type owl:Class ;
  rdfs:label "Model variable"^^xsd:string ;
  rdfs:subClassOf :Column ;
  skos:definition "A CDISC model variable. Building block for a class variable (shared across classes)."^^xsd:string ;
.
:ClassDomain
  rdf:type owl:Class ;
  rdfs:label "Class Domain"^^xsd:string ;
  rdfs:subClassOf :Tabulation ;
  skos:definition "A CDISC domain class. Building block for a SDTM IG domain."^^xsd:string ;
.
:ClassVariable
  rdf:type owl:Class ;
  rdfs:label "Class variable"^^xsd:string ;
  rdfs:subClassOf :Column ;
  skos:definition "A CDISC domain class variable. Building block for a SDTM IG variable."^^xsd:string ;
.
:IgDomain
  rdf:type owl:Class ;
  rdfs:label "Implementation Guide Domain"^^xsd:string ;
  rdfs:subClassOf :Tabulation ;
