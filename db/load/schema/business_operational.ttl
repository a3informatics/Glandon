@prefix : <http://www.assero.co.uk/BusinessOperational#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoT: <http://www.assero.co.uk/ISO11179Types#> .
@prefix th: <http://www.assero.co.uk/Thesaurus#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/BusinessOperational>
  rdf:type owl:Ontology ;
  owl:versionInfo "v3.3.0"^^xsd:string ;
  rdfs:label "Operational Schema"^^xsd:string ;
.
:Component
  rdf:type owl:Class ;
  rdfs:label "Component"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ; # Deprecated, to be removed.
  rdfs:subClassOf isoT:AdministeredItem ;
  skos:definition "Generic reference type."^^xsd:string ;
.
:Collection
  rdf:type owl:Class ;
  rdfs:label "Collection"^^xsd:string ;
  rdfs:subClassOf :Component ;
  skos:definition "Generic collection type."^^xsd:string ;
.
:Reference
  rdf:type owl:Class ;
  rdfs:label "Reference"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Generic reference type."^^xsd:string ;
.
:Association
  rdf:type owl:Class ;
  rdfs:label "Association"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Generic association type."^^xsd:string ;
.
:BcReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept."^^xsd:string ;
.
:BctReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Template Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept Template."^^xsd:string ;
.
:BcPReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Property Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept Property."^^xsd:string ;
.
:TUcReference
  rdf:type owl:Class ;
  rdfs:label "Thesaurus Unmanaged Concept Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Thesaurus Unmanaged Concept."^^xsd:string ;
.
:TMcReference
  rdf:type owl:Class ;
  rdfs:label "Thesaurus Managed Concept Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Thesaurus Managed Concept."^^xsd:string ;
.
:TReference
  rdf:type owl:Class ;
  rdfs:label "Tabulation Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Tabulation."^^xsd:string ;
.
:CReference
  rdf:type owl:Class ;
  rdfs:label "Tabulation Column Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Tabulation Column."^^xsd:string ;
.
:hasManaged
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Has Managed"^^xsd:string ;
  rdfs:domain :Collection ;
  rdfs:range :Reference ;
  skos:definition "Relationship to a reference."^^xsd:string ;
.
:reference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Branched From."^^xsd:string ;
  rdfs:domain :Reference ;
  rdfs:range isoC:Concept ;
  skos:definition "Relationship to a referenced concept."^^xsd:string ;
.
:theSubject
  rdf:type owl:ObjectProperty ;
  rdfs:label "Subject"^^xsd:string ;
  rdfs:domain :Association ;
  rdfs:range :Component ;
  skos:definition "Relationship to the component which is the subject of an association."^^xsd:string ;
.
:associatedWith
  rdf:type owl:ObjectProperty ;
  rdfs:label "Associated With"^^xsd:string ;
  rdfs:domain :Association ;
  rdfs:range :Component ;
  skos:definition "Relationship to the components with which the subject is associated."^^xsd:string ;
.
:semantic
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Semantic"^^xsd:string ;
  rdfs:domain :Association ;
  rdfs:range xsd:string ;
  skos:definition "The reason for the association, the semantic."^^xsd:string ;
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :Component :Reference )
    ] ;
  rdfs:label "ordinal"^^xsd:string ;
  rdfs:range xsd:positiveInteger ;
  skos:definition "The releative position within the collection."^^xsd:string ;
.
:enabled
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Reference ;
  rdfs:label "enabled"^^xsd:string ;
  rdfs:range xsd:boolean ;
  skos:definition "The reference enabled flag. True if enabled, false otherwise."^^xsd:string ;
.
:optional
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Reference ;
  rdfs:label "optional"^^xsd:string ;
  rdfs:range xsd:boolean ;
  skos:definition "The reference optional flag. True if optiomal, false if required."^^xsd:string ;
.
:localLabel
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :TUcReference ;
  rdfs:label "Local Label"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "The local label. Used to overwrite the referenced item's label."^^xsd:string ;
.