# baseURI: http://www.assero.co.uk/BusinessCrossReference
# imports: http://www.assero.co.uk/ISO11179Concepts

@prefix : <http://www.assero.co.uk/BusinessCrossReference#> .
@prefix bo: <http://www.assero.co.uk/BusinessOperational#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoT: <http://www.assero.co.uk/ISO11179Types#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/BusinessCrossReference>
  rdf:type owl:Ontology ;
  owl:imports <http://www.assero.co.uk/ISO11179Concepts> ;
  owl:versionInfo "V1"^^xsd:string ;
.
:CrossReference
  rdf:type owl:Class ;
  rdfs:label "Cross Reference"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Holds the set of cross references."^^xsd:string ;
.
:comments
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Comments"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:string ;
  skos:definition "The comments regarding the cross references."^^xsd:string
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Ordinal"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:positiveInteger ;
  skos:definition "The ordering of the cross references within the collection."^^xsd:string
.
:crossReference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Cross Reference relationship."^^xsd:string ;
  rdfs:domain isoC:Concept ;
  rdfs:range :CrossReference ;
  skos:definition "A cross reference relationship between managed items."^^xsd:string ;
.
:hasCrossReference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Cross Reference relationship."^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range bo:Xreference ;
  skos:definition "A cross reference relationship between managed items."^^xsd:string ;
.
