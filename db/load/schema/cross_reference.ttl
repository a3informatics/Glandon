@prefix : <http://www.assero.co.uk/CrossReference#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoT: <http://www.assero.co.uk/ISO11179Types#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/CrossReference>
  rdf:type owl:Ontology ;
.
:CrossReference
  rdf:type owl:Class ;
  rdfs:label "Cross Reference"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Generic cross reference type."^^xsd:string ;
.
:ChangeInstruction
  rdf:type owl:Class ;
  rdfs:label "Change Instruction"^^xsd:string ;
  rdfs:subClassOf :CrossReference ;
  skos:definition "Change instruction type."^^xsd:string ;
.
:semantic
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Cross Reference Semantic"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:string ;
  skos:definition "The semantic of the cross reference."^^xsd:string ;
.
:description
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Description"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:string ;
  skos:definition "The description about why the items are cross referenced."^^xsd:string ;
.
:canBeDeleted
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Can Be Deleted"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:boolean ;
  skos:definition "The cross reference can be deleted flag."^^xsd:string ;
.
:canBeModified
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Can Be Modified"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:boolean ;
  skos:definition "The cross reference can be modified flag."^^xsd:string ;
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Ordinal"^^xsd:string ;
  rdfs:domain :CrossReference ;
  rdfs:range xsd:positiveInteger ;
  skos:definition "The position within the collection."^^xsd:string ;
.
:previousReference
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Previous Reference"^^xsd:string ;
  rdfs:domain :ChangeInstruction ;
  rdfs:range isoC:Concept ;
  skos:definition "A previous reference."^^xsd:string ;
.
:currentReference
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Current Reference"^^xsd:string ;
  rdfs:domain :ChangeInstruction ;
  rdfs:range isoC:Concept ;
  skos:definition "A current reference."^^xsd:string ;