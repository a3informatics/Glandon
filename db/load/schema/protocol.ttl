@prefix : <http://www.assero.co.uk/Protocol#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix bo: <http://www.assero.co.uk/BusinessOperational#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/Protocol>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.assero.co.uk/ISO11179Concepts> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
:Study
  rdf:type owl:Class ;
  rdfs:subClassOf bo:Component ;
  rdfs:label "Thesaurus"^^xsd:string ;
  rdfs:subClassOf skos:ConceptScheme ;
  skos:definition "A thesaurus. A SKOS concept scheme"^^xsd:string ;
.
:Protocol
  rdf:type owl:Class ;
  rdfs:subClassOf bo:Component ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Concept"^^xsd:string ;
  skos:definition "A member of a thesaurus, a concept. Version managed."^^xsd:string ;
.
:implements
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "implements Relationship"^^xsd:string ;
  rdfs:domain  :Protocol ;
  rdfs:range :Study ;
  skos:definition "Protocol implements a study relationship."^^xsd:string ;
.
:name
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:subPropertyOf skos:definition ;
  rdfs:label "Name"^^xsd:string ;
  rdfs:domain  :Study ;
  rdfs:range xsd:string ;
  skos:definition "The study's name."^^xsd:string ;
.
