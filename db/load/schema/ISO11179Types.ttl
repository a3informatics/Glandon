# baseURI: http://www.assero.co.uk/ISO11179Types
# imports: http://purl.org/dc/elements/1.1/
# imports: http://purl.org/dc/terms/
# imports: http://www.w3.org/2004/02/skos/core

@prefix : <http://www.assero.co.uk/ISO11179Types#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/ISO11179Types>
  rdf:type owl:Ontology ;
  owl:versionInfo "v3.2.0"^^xsd:string ;
  rdfs:label "ISO11179 Types Schema"^^xsd:string ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
:AdministeredItem
  rdf:type owl:Class ;
  rdfs:label "Administered item"^^xsd:string ;
  rdfs:subClassOf :RegisteredItem ;
  skos:definition "An item that is administered within the registry."^^xsd:string ;
.
# Deprecate
:AttachedItem
  rdf:type owl:Class ;
  rdfs:label "Attached item"^^xsd:string ;
  rdfs:subClassOf :RegisteredItem ;
.
:RegisteredItem
  rdf:type owl:Class ;
  rdfs:label "Registered item"^^xsd:string ;
  rdfs:subClassOf :IdentifiedItem ;
  skos:definition "An item that is registered within the registry."^^xsd:string ;
.
:IdentifiedItem
  rdf:type owl:Class ;
  rdfs:label "Identified item"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "An item that can be retrieved directly."^^xsd:string ;
.
# Deprecate
:DesignatableItem
  rdf:type owl:Class ;
  rdfs:label "Registered item"^^xsd:string ;
  rdfs:subClassOf :DesgnatableItem ;
  skos:definition "An item that is designated within the registry."^^xsd:string ;
.
:creationDate
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Creation Date"^^xsd:string ;
  rdfs:range xsd:dateTime ;
  skos:definition "The date the Administered Item was created."^^xsd:string ;
.
:lastChangeDate
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Last Change Date"^^xsd:string ;
  rdfs:range xsd:dateTime ;
  skos:definition "The date the Administered Item was last changed."^^xsd:string ;
.
:changeDescription
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Change Description"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "A description of what has changed since the last version of the Administered Item."^^xsd:string ;
.
:explanatoryComment
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Explanatory Comment"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Any descriptive comments about the item."^^xsd:string ;
.
:origin
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "origin"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Any notes about the origin (reference material etc) of the item."^^xsd:string ;
. 
# Deprecate
:notation
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :DesignatableItem ;
  rdfs:label "Notation"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Formal syntax used within the concept system"^^xsd:string ;
.
# Deprecate
:identifer
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :DesignatableItem ;
  rdfs:label "Identifier"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "Identifier for the concept system."^^xsd:string ;
.
:hasIdentifier
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :IdentifiedItem ;
  rdfs:label "Identifer Relationship"^^xsd:string ;
  rdfs:range :ScopedIdentifier ;
  skos:definition "Identifies the set of Scope_Identifiers for the Identified_Item that defines an identifier within a specific namespace."^^xsd:string ;
.
:hasState
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Item Registration Relationship"^^xsd:string ;
  rdfs:range :RegistrationState ;
  skos:definition "Identifies the state of the registered item."^^xsd:string ;
.