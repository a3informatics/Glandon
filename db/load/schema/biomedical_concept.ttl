@prefix : <http://www.assero.co.uk/BiomedicalConcept#> .
@prefix bo: <http://www.assero.co.uk/BusinessOperational#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> . 
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/BiomedicalConcept>
  rdf:type owl:Ontology ;
.
:BiomedicalConcept
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "A Biomedical Concept. An observation."^^xsd:string ;
.
:BiomedicalConceptInstance
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Instance"^^xsd:string ;
  rdfs:subClassOf :BiomedicalConcept ;
  skos:definition "A Biomedical Concept instance."^^xsd:string ;
.
:BiomedicalConceptTemplate
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Template"^^xsd:string ;
  rdfs:subClassOf :BiomedicalConcept ;
  skos:definition "A template (or pattern) for a Biomedical Concept."^^xsd:string ;
.
:Item
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Biomedical Concept Item"^^xsd:string ;
  skos:definition "An item within a Biomedical Concept."^^xsd:string ;
.
:ComplexDatatype
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Biomedical Concept Complex Datatype"^^xsd:string ;
  skos:definition "A complex datatype within a Biomedical Concept."^^xsd:string ;
.
:Property
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Biomedical Concept Property"^^xsd:string ;
  skos:definition "A datatype property within a Biomedical Concept."^^xsd:string ;
.
:hasCodeValue
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :Property ;
  rdfs:range bo:TMcReference ;
  rdfs:label "Has coded value relationship"^^xsd:string ;
  skos:definition "Reference to a coded value."^^xsd:string
.
:hasComplexDatatype
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :Property ;
  rdfs:label "Has a complext data type"^^xsd:string ;
  rdfs:range :Datatype ;
  owl:propertyDisjointWith :hasValue ;
.
:hasItem
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Link to the constiuent parts of the Biomedical Concept (Instance or Template)"^^xsd:string ;
  rdfs:domain :BiomedicalConcept ;
  rdfs:range :Item ;
.
:hasProperty
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :Datatype ;
  rdfs:label "Has A Property"^^xsd:string ;
  rdfs:range :Property ;
  owl:inverseOf :isPropertyOf ;
.
:alias
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Property ;
  rdfs:range xsd:string ;
  rdfs:label "Alias"^^xsd:string ;
  skos:definition "A short hand name for the item to allow understanding"^^xsd:string ;
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Item ;
  rdfs:label "Ordinal"^^xsd:string ;
  rdfs:range xsd:positiveInteger ;
  skos:definition "Order of the node within the collection"^^xsd:string ;
.
:basedOn
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Based on"^^xsd:string ;
  rdfs:domain :BiomedicalConceptInstance ;
  rdfs:range bo:reference ;
  skos:definition "Relationship to relate Biomedical Concepts with the defining Template."^^xsd:string ;
.
:questionText
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Question Text"^^xsd:string ;
  rdfs:domain :Property ;
  rdfs:range xsd:string ;
  skos:definition "The question text for a property."^^xsd:string ;
.
:promptText
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Prompt Text"^^xsd:string ;
  rdfs:domain :Property ;
  rdfs:range xsd:string ;
  skos:definition "The prompt text for a property."^^xsd:string ;
.
:format
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Property ;
  rdfs:label "The format for the reponse"^^xsd:string ;
  rdfs:range xsd:string ;
.
:enabled
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Enabled"^^xsd:string ;
  rdfs:domain :Property ;
  rdfs:range xsd:boolean ;
  skos:definition "The property is enabled."^^xsd:string ;
.
:mandatory
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Mandatory"^^xsd:string ;
  rdfs:domain :Property ;
  rdfs:range xsd:boolean ;
  skos:definition "The property is mandatory."^^xsd:string ;
.
:collect
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Collect"^^xsd:string ;
  rdfs:domain :Property ;
  rdfs:range xsd:boolean ;
  skos:definition "The property can be collected (rather than a fixed value)"^^xsd:string ;
.