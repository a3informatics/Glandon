# Administered Item Copy
#
#
:AdministeredItem
  rdf:type owl:Class ;
  rdfs:label "Administered item"^^xsd:string ;
  rdfs:subClassOf :RegisteredItem ;
  skos:definition "An item that is administered within the registry."^^xsd:string ;
.
:changeDescription
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Change Description"^^xsd:string ;
  rdfs:range xsd:string ;
  skos:definition "A description of what has changed since the last version of the Administered Item."^^xsd:string ;
.
:hasIdentifier
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :IdentifiedItem ;
  rdfs:label "Identifer Relationship"^^xsd:string ;
  rdfs:range :ScopedIdentifier ;
  skos:definition "Identifies the set of Scope_Identifiers for the Identified_Item that defines an identifier within a specific namespace."^^xsd:string ;
.
:hasState
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :AdministeredItem ;
  rdfs:label "Item Registration Relationship"^^xsd:string ;
  rdfs:range :RegistrationState ;
  skos:definition "Identifies the state of the registered item."^^xsd:string ;
.