# Form Migration
#
# Adds in BcGroup

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

:BcGroup
  rdf:type owl:Class ;
  rdfs:label "BC Group"^^xsd:string ;
  rdfs:subClassOf :Group ;
  skos:definition "BC group."^^xsd:string
.
:hasBiomedicalConcept
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BcGroup ;
  rdfs:label "Reference to Biomedical Concept"^^xsd:string ;
  rdfs:range bo:BcReference ;
  skos:definition "Reference to a Biomedical Concept"^^xsd:string
.
:hasCommon
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BcGroup ;
  rdfs:label "Has common group relationship"^^xsd:string ;
  rdfs:range :CommonGroup ;
  skos:definition "Has a common group relationship"^^xsd:string
.
:hasSubGroup
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :NormalGroup ;
  rdfs:label "Has sub group relationship"^^xsd:string ;
  rdfs:range [ a owl:Class ;
    owl:unionOf  (:NormalGroup :BcGroup)
    ] ;
  skos:definition "Has a sub group relationship"^^xsd:string
.