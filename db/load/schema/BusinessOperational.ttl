@prefix : <http://www.assero.co.uk/BusinessOperational#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix cbc: <http://www.assero.co.uk/CDISCBiomedicalConcept#> .
@prefix iso25964: <http://www.assero.co.uk/ISO25964#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/BusinessOperational>
  rdf:type owl:Ontology ;
  owl:imports <http://www.assero.co.uk/ISO11179Concepts> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:Component
  rdf:type owl:Class ;
  rdfs:label "Component"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
.
:Reference
  rdf:type owl:Class ;
  rdfs:label "Reference"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "Generic reference type."^^xsd:string ;
.
:BcReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept."^^xsd:string ;
.
:BctReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Template Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept Template."^^xsd:string ;
.
:PReference
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept Property Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Biomedical Concept property."^^xsd:string ;
.
:TcReference
  rdf:type owl:Class ;
  rdfs:label "Thesaurus Concept Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Thesaurus Concept."^^xsd:string ;
.
:TReference
  rdf:type owl:Class ;
  rdfs:label "Tabulation Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Tabulation."^^xsd:string ;
.
:CReference
  rdf:type owl:Class ;
  rdfs:label "Tabulation Column Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to a Tabulation Column."^^xsd:string ;
.
:BReference
  rdf:type owl:Class ;
  rdfs:label "Branch Reference"^^xsd:string ;
  rdfs:subClassOf :Reference ;
  skos:definition "Holds the reference to the branch parent."^^xsd:string ;
.
:hasBiomedicalConcept
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BcReference ;
  rdfs:label "Biomedical Concept relationship."^^xsd:string ;
  rdfs:range cbc:BiomedicalConceptInstance ;
  skos:definition "Biomedical Concept instance relationship."^^xsd:string ;
.
:basedOnTemplate
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BcReference ;
  rdfs:label "Biomedical Concept Templaterelationship."^^xsd:string ;
  rdfs:range cbc:BiomedicalConceptTemplate ;
  skos:definition "Biomedical Concept Template relationship."^^xsd:string ;
.
:hasProperty
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BcReference ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :PReference :BcReference )
    ] ;
  rdfs:label "Biomedical Concept property relationship."^^xsd:string ;
  rdfs:range cbc:Property ;
  skos:definition "Biomedical Concept property relationship."^^xsd:string ;
.
:hasThesaurusConcept
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :TcReference ;
  rdfs:label "Thesaurus Concept relationship."^^xsd:string ;
  rdfs:range iso25964:ThesaurusConcept ;
  skos:definition "Thesaurus Concept relationship."^^xsd:string ;
.
:hasTabulation
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :TReference ;
  rdfs:label "Tabulation relationship."^^xsd:string ;
  rdfs:range iso25964:ThesaurusConcept ;
  skos:definition "Tabulation relationship."^^xsd:string ;
.
:hasColumn
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :CReference ;
  rdfs:label "Tabulation Column relationship."^^xsd:string ;
  rdfs:range iso25964:ThesaurusConcept ;
  skos:definition "Tabulation Column relationship."^^xsd:string ;
.
:branchedFrom
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:domain :BReference ;
  rdfs:label "Branched From relationship."^^xsd:string ;
  rdfs:range :Component ;
  skos:definition "Branched From relationship."^^xsd:string ;
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :Component :Reference )
    ] ;
  rdfs:label "ordinal"^^xsd:string ;
  rdfs:range xsd:positiveInteger ;
.
:enabled
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Reference ;
  rdfs:label "enabled"^^xsd:string ;
  rdfs:range xsd:boolean ;
.
:optional
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :Reference ;
  rdfs:label "optional"^^xsd:string ;
  rdfs:range xsd:boolean ;
.
:local_label
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:domain :TcReference ;
  rdfs:label "Repelacement label"^^xsd:string ;
  rdfs:range xsd:string ;
.
