@prefix : <http://www.assero.co.uk/Datatypes#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoR: <http://www.assero.co.uk/ISO11179Registration#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/Datatypes>
  rdf:type owl:Ontology ;
.
:Datatype
  rdf:type owl:Class ;
  rdfs:label "Datatype"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "A simple datatype."^^xsd:string ;
.
:xsd
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Schema datatype"^^xsd:string ;
  rdfs:domain :Datatype ;
  rdfs:range xsd:string ;
  skos:definition "The XSD datatyep URI."^^xsd:string ;
.
:shortLabel
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Short label"^^xsd:string ;
  rdfs:domain :Datatype ;
  rdfs:range xsd:string ;
  skos:definition "A short label for the datatype, typically one or two characters."^^xsd:string ;
.
:odm
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "ODM Equivalent"^^xsd:string ;
  rdfs:domain :Datatype ;
  rdfs:range xsd:string ;
  skos:definition "The ODM equivalent datatype"^^xsd:string ;
.
:literalMethod
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Literal output format"^^xsd:string ;
  rdfs:domain :Datatype ;
  rdfs:range xsd:string ;
  skos:definition "The output format for literals"^^xsd:string ;
.
:typeMethod
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Literal output format"^^xsd:string ;
  rdfs:domain :Datatype ;
  rdfs:range xsd:string ;
  skos:definition "The output format for literals"^^xsd:string ;