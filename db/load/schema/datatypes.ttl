@prefix : <http://www.assero.co.uk/Datatypes#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoR: <http://www.assero.co.uk/ISO11179Registration#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.assero.co.uk/Datatypes>
  rdf:type owl:Ontology ;
.
:ComplexDatatype
  rdf:type owl:Class ;
  rdfs:label "Complex Datatype"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "A complex datatype."^^xsd:string ;
.
:Attribute
  rdf:type owl:Class ;
  rdfs:label "Complex Datatype Attribute"^^xsd:string ;
  rdfs:subClassOf isoC:Concept ;
  skos:definition "An attribute of a complex datatype."^^xsd:string ;
.
:hasAttribute
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Attribute"^^xsd:string ;
  rdfs:domain :ComplexDatatype ;
  rdfs:range :Attribute ;
  skos:definition "Reference to an attribute."^^xsd:string ;
.