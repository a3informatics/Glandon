@prefix : <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoI: <http://www.assero.co.uk/ISO11179Identification#> .
@prefix isoT: <http://www.assero.co.uk/ISO11179Types#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/ISO11179Concepts>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.assero.co.uk/ISO11179Identification> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:Concept
  rdf:type owl:Class ;
  rdfs:label "Concept"^^xsd:string ;
  skos:definition "A unit of knowledge created by a unique combination of characteristics."^^xsd:string ;
.
:ConceptSystem
  rdf:type owl:Class ;
  rdfs:label "Concept System"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Set of concepts structured according to the relations among them"^^xsd:string ;
.
:Classification
  rdf:type owl:Class ;
  rdfs:label "Classification"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Set of concepts structured according to the relations among them"^^xsd:string ;
.
:ClassifiableItem
  rdf:type owl:Class ;
  rdfs:label "Classificable Item"^^xsd:string ;
  rdfs:subClassOf :Concept ;
  skos:definition "An item that is classifiable within the repository"^^xsd:string ;
.
:Property
  rdf:type owl:Class ;
  rdfs:label "Property"^^xsd:string ;
  rdfs:subClassOf :Concept ;
  skos:definition "Quality common to all members of an object class"^^xsd:string ;
.
:schemeContains
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ConceptSystem ;
  rdfs:label "hasClassification Relationship"^^xsd:string ;
  rdfs:range :Classification ;
  skos:definition "???"^^xsd:string ;
.
:inScheme
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Classification ;
  rdfs:label "inScheme Relationship"^^xsd:string ;
  rdfs:range :ConceptSystem ;
  skos:definition "???"^^xsd:string ;
.
:classifiedAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Classification ;
  rdfs:label "classifiedAs Relationship"^^xsd:string ;
  rdfs:range :Concept ;
  skos:definition "???"^^xsd:string ;
.
:isClassifying
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Concept ;
  rdfs:label "ClassificationRelationship Relationship"^^xsd:string ;
  rdfs:range :Classification ;
  skos:definition "???"^^xsd:string ;
.
:isMemberOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Concept ;
  rdfs:label "Member Concept Relationship"^^xsd:string ;
  rdfs:range :ConceptSystem ;
  skos:definition "???"^^xsd:string ;
.
:hasMember
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ConceptSystem ;
  rdfs:label "Sourced Concept Relationship"^^xsd:string ;
  rdfs:range :Concept ;
  skos:definition "???"^^xsd:string ;
.
:link
  rdf:type owl:ObjectProperty ;
  rdfs:label "Link"^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range :Concept ;
  skos:definition "The relationship between Concepts"^^xsd:string ;
.
:hasRelation
  rdf:type owl:ObjectProperty ;
  rdfs:domain isoT:ClassifiableItem ;
  rdfs:label "ConceptSystemClassification Relationship"^^xsd:string ;
  rdfs:range :Classification ;
  skos:definition "???"^^xsd:string ;
.
:isRelatedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Classification ;
  rdfs:label "ClassificationScheme Relationship"^^xsd:string ;
  rdfs:range isoT:ClassifiableItem ;
  skos:definition "???"^^xsd:string ;
.
:property
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Concept ;
  rdfs:label "Concept property"^^xsd:string ;
  rdfs:range xsd:anySimpleType ;
  skos:definition "The property for a concept"^^xsd:string ;

.