.
:Node
  rdf:type owl:Class ;
  rdfs:label "Group"^^xsd:string ;
.
:Group
  rdf:type owl:Class ;
  rdfs:label "Group"^^xsd:string ;
  rdfs:subClassOf :Node ;
.
:Item
  rdf:type owl:Class ;
  rdfs:label "Generic Item"^^xsd:string ;
  rdfs:subClassOf :Node ;
.
:Placeholder
  rdf:type owl:Class ;
  rdfs:label "Placeholder Item"^^xsd:string ;
  rdfs:subClassOf :Item ;
.
:bcBased
  rdf:type owl:Class ;
  rdfs:label "Biomedical Concept based Item"^^xsd:string ;
  rdfs:subClassOf :Item ;
.
:vBased
  rdf:type owl:Class ;
  rdfs:label "Variable Item"^^xsd:string ;
  rdfs:subClassOf :Item ;
.
:hasGroupRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Form ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range :Group ;
.
:isGroupOfRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Group ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range :Form ;
.
:hasItemRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Group ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range :Item ;
.
:isItemOfRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Item ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range :Group ;
.
:hasPropertyRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Item ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range cbc:Property ;
.
:hasBiomedicalConceptRelationship
  rdf:type owl:ObjectProperty ;
  rdfs:domain :bcBased ;
  rdfs:label "Has group relationship"^^xsd:string ;
  rdfs:range cbc:BiomedicalConcept ;
.
:name
  rdf:type owl:DatatypeProperty ;
  rdfs:label "name"^^xsd:string ;
  rdfs:domain :Node ;
  rdfs:range xsd:string ;
.
:layout
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Group ;
  rdfs:label "presentation"^^xsd:string ;
  rdfs:range xsd:string ;
.
:text
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Label ;
  rdfs:label "text"^^xsd:string ;
  rdfs:range xsd:string ;
.
:ordinal
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Label ;
  rdfs:label "text"^^xsd:string ;
  rdfs:range xsd:integer ;
