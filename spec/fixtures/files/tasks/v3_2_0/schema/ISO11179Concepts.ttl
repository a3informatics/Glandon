@prefix : <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix isoT: <http://www.assero.co.uk/ISO11179Types#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/ISO11179Concepts>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.assero.co.uk/ISO11179Identification> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:Concept
  rdf:type owl:Class ;
  rdfs:label "Concept"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A unit of knowledge created by a unique combination of characteristics."^^xsd:string ;
.
:Extension
  rdf:type owl:Class ;
  rdfs:label "Extension"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "An extension to a concept defining the characteristics of the property."^^xsd:string ;
.
:ConceptSystem
  rdf:type owl:Class ;
  rdfs:label "Concept System"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf skos:ConceptScheme ;
  skos:definition "Set of concepts structured according to the relations among them."^^xsd:string ;
.
:ConceptSystemNode
  rdf:type owl:Class ;
  rdfs:label "Concept System Node"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf skos:Concept ;
  skos:definition "A node in a concept system."^^xsd:string ;
.
:link
  rdf:type owl:ObjectProperty ;
  rdfs:label "Link"^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range :Concept ;
  skos:definition "The relationship between Concepts"^^xsd:string ;
.
:property
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Property"^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range xsd:anySimpleType ;
  skos:definition "The property for a concept"^^xsd:string ;
.
:label
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf rdfs:label ; # To ensure seen as a RDFS lable as well.
  rdfs:subPropertyOf :property ;
  rdfs:label "Label"^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range xsd:string ;
  skos:definition "The label for a concept"^^xsd:string ;
.
# Deprecated
:extensionProperty
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Concept ;
  rdfs:label "Concept extension property"^^xsd:string ;
  rdfs:range xsd:anySimpleType ;
  skos:definition "A user-defined extension property for a concept"^^xsd:string ;
.
:narrower
  rdf:type owl:ObjectProperty ;
  rdfs:label "Narrower relationship."^^xsd:string ;
  rdfs:subPropertyOf :link ;
  rdfs:subPropertyOf skos:narrower ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :ConceptSystem :ConceptSystemNode )
    ] ;
  rdfs:range :ConceptSystemNode ;
  skos:definition "Relationship to relate the nodes within a concept system."^^xsd:string ;
.
# Deprecated
:hasMember
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf :link ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :ConceptSystem :ConceptSystemNode )
    ] ;
  rdfs:label "Has member relationship."^^xsd:string ;
  rdfs:range :ConceptSystemNode ;
  skos:definition "Relationship to relate the nodes within a concept system."^^xsd:string ;
.
:tagged
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf :link ;
  rdfs:label "Is tagged with relationship."^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range :conceptSystemNode ;
  skos:definition "Relationship to relate a concept to its tags."^^xsd:string ;
.
:prefLabel
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf :property ;
  rdfs:subPropertyOf skos:prefLabel ;
  rdfs:label "Label"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :ConceptSystem :ConceptSystemNode )
    ] ;
  rdfs:range xsd:string ;
  skos:definition "The concept's preferred label."^^xsd:string ;
.
:description
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Concept system node description."^^xsd:string ;
  rdfs:subPropertyOf :property ;
  rdfs:subPropertyOf skos:definition ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :ConceptSystem :ConceptSystemNode )
    ] ;
  rdfs:range xsd:string ;
  skos:definition "Concept system node description."^^xsd:string ;
.
:extended
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf :link ;
  rdfs:label "Is extended relationship."^^xsd:string ;
  rdfs:domain :Concept ;
  rdfs:range :Extension ;
  skos:definition "Relationship to relate a concept to the extension properties."^^xsd:string ;
.
:extensionName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Extension Name"^^xsd:string ;
  rdfs:domain :Extension ;
  rdfs:range xsd:string ;
  skos:definition "The name of the extension property."^^xsd:string ;
.
:extensionDefault
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Extension Default Value"^^xsd:string ;
  rdfs:domain :Extension ;
  rdfs:range xsd:anySimpleType ;
  skos:definition "The default value for the extension property."^^xsd:string ;
.
:extensionDatatype
  rdf:type owl:DatatypeProperty ;
  rdfs:label "Extension Datatype"^^xsd:string ;
  rdfs:domain :Extension ;
  rdfs:range xsd:anySimpleType ;
  skos:definition "The datatype of the extension property."^^xsd:string ;
.