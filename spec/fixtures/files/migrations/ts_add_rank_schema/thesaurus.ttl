@prefix : <http://www.assero.co.uk/Thesaurus#> .
@prefix isoC: <http://www.assero.co.uk/ISO11179Concepts#> .
@prefix bo: <http://www.assero.co.uk/BusinessOperational#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.assero.co.uk/Thesaurus>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.assero.co.uk/ISO11179Concepts> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
:Thesaurus
  rdf:type owl:Class ;
  rdfs:subClassOf bo:Component ;
  rdfs:label "Thesaurus"^^xsd:string ;
  rdfs:subClassOf skos:ConceptScheme ;
  skos:definition "A thesaurus. A SKOS concept scheme"^^xsd:string ;
.
:ManagedConcept
  rdf:type owl:Class ;
  rdfs:subClassOf bo:Component ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Concept"^^xsd:string ;
  skos:definition "A member of a thesaurus, a concept. Version managed."^^xsd:string ;
.
:UnmanagedConcept
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Concept System"^^xsd:string ;
  skos:definition "A member of a thesaurus, a concept. Not version managed."^^xsd:string ;
.
:Subset
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Subset"^^xsd:string ;
  rdfs:subClassOf skos:OrderedCollection ;
  skos:definition "Thesaurus Concept"^^xsd:string ;
.
:SubsetMember
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Subset"^^xsd:string ;
  rdfs:subClassOf rdf:List ;
  skos:definition "Thesaurus Concept"^^xsd:string ;
.
:PreferredTerm
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Preferred Term"^^xsd:string ;
  rdfs:subClassOf skosxl:Label ;
  skos:definition "A single preferred term, a preferred label"^^xsd:string ;
.
:Synonym
  rdf:type owl:Class ;
  rdfs:subClassOf isoC:Concept ;
  rdfs:label "Synonym"^^xsd:string ;
  rdfs:subClassOf skosxl:Label ;
  skos:definition "A single synonym, an alternative label"^^xsd:string ;
.
:isTopConceptReference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Top concept reference relationship"^^xsd:string ;
  rdfs:domain :Thesaurus ;
  rdfs:range bo:TMcReference ;
  skos:definition "Relationship linking a thesaurus to concepts that are themseleves version managed."^^xsd:string ;
.
:isTopConcept
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:subPropertyOf skos:hasTopConcept ;
  rdfs:label "Top concept relationship"^^xsd:string ;
  rdfs:domain :Thesaurus ;
  rdfs:range :ManagedConcept ;
  skos:definition "Relationship linking a thesaurus to child concepts to preserve the SKOS model."^^xsd:string ;
.
:narrowerReference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Narrower reference relationship"^^xsd:string ;
  rdfs:domain :ManagedConcept ;
  rdfs:range bo:TcReference ;
  skos:definition "Relationship linking a managed concept to concepts that are themseleves version managed."^^xsd:string ;
.
:narrower
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:subPropertyOf skos:narrower ;
  rdfs:label "hasChild Relationship"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range :UnmanagedConcept ;
  skos:definition "Concepts narrower relationship."^^xsd:string ;
.
:extends
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:subPropertyOf skos:narrower ;
  rdfs:label "Extends relationship"^^xsd:string ;
  rdfs:domain :ManagedConcept ;
  rdfs:range :ManagedConcept ;
  skos:definition "A relationship linking the concept with the one being extended."^^xsd:string ;
.
:subsets
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:subPropertyOf skos:narrower ;
  rdfs:label "Subsets relationship"^^xsd:string ;
  rdfs:domain :ManagedConcept ;
  rdfs:range :ManagedConcept ;
  skos:definition "A relationship linking the concept with the one being subsetted."^^xsd:string ;
.
:reference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Reference relationship"^^xsd:string ;
  rdfs:domain :Thesaurus ;
  rdfs:range bo:Reference  ;
  skos:definition "A relationship linking a thesaurus with a reference thesaurus."^^xsd:string ;
.
:baselineReference
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf isoC:link ;
  rdfs:label "Baseline reference relationship"^^xsd:string ;
  rdfs:domain :Thesaurus ;
  rdfs:range bo:Reference  ;
  skos:definition "A relationship linking a thesaurus with a baseline reference thesaurus. used for upgrades"^^xsd:string ;
.
:definition
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:subPropertyOf skos:definition ;
  rdfs:label "Definition"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range xsd:string ;
  skos:definition "The concept's definition."^^xsd:string ;
.
:identifier
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Identifier"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range xsd:string ;
  skos:definition "The concept's identifier."^^xsd:string ;
.
:notation
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:subPropertyOf skos:notation ;
  rdfs:label "Notation"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range xsd:string ;
  skos:definition "The concept's notation (coded value). In the CDISC terminology this is called the submission value."^^xsd:string ;
.
:extensible
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Extensible"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range xsd:boolean ;
  skos:definition "Is the concept extensible. Set to true if the collection can be extended, false otherwise."^^xsd:string ;
.
:preferredTerm
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Preferred Term"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range :PreferredTerm ;
  skos:definition "Relationship to the preferred term."^^xsd:string ;
.
:synonym
  rdf:type owl:DatatypeProperty  ;
  rdfs:subPropertyOf isoC:property ;
  rdfs:label "Synonym"^^xsd:string ;
  rdfs:domain  [ a owl:Class ;
    owl:unionOf  ( :UnmanagedConcept :ManagedConcept )
    ] ;
  rdfs:range :Synonym ;
  skos:definition "Relationship to a synonym."^^xsd:string ;
.
