<BiomedicalConceptTemplates Version="1">
	<BiomedicalConceptTemplate Id="BCT MH" Name="Medical History Biomedical Concept Template">
		<Class Name="PerformedObservationResult">
			<Attribute Alias="Result" Name="value" Datatype="ST">
				<Property Alias="Name (--TERM)"  Name="value"/>
			</Attribute>
		</Class>
		<Class Name="DefinedObservation">
			<Attribute Alias="Category" Name="categoryCode">
				<Property Alias="Category (--CAT)" Name="code"/>
			</Attribute>
			<Attribute Alias="Subcategory" Name="subcategoryCode">
				<Property Alias="Subcategory (--SCAT)" Name="code"/>
			</Attribute>
		</Class>
		<Class Name="PerformedObservation"> 
			<Attribute Alias="Not Done" Name="negationIndicator">
				<Property Alias="Not Done (--STAT)" Name="value"/>
			</Attribute>
			<Attribute Alias="Reason Not Done" Name="negationReason">
				<Property Alias="Code"  Name="code">
					<Property Alias="Reason Not Done (--REASND)" Name="code"/>
				</Property>
			</Attribute>
			<Attribute Alias="DateTime" Name="dateRange">
				<Property Alias="Date" Name="low">
					<Property Alias="Date Time (--DTC)" Name="value"></Property>
				</Property>
			</Attribute>
		</Class>
	</BiomedicalConceptTemplate>
</BiomedicalConceptTemplates>

